      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,3567,1386,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Never in the kingdom or the empire",
