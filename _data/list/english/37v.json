    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And nowhere did they see",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,2949,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Anyone who seemed to want to joust.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3052,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "They went from place to place so much",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3155,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That they saw Fauvel’s people,",
        "language": "english"
      },
