      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,3459,1386,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Their inn. But to tell the truth,",
