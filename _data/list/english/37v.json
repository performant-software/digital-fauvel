    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And so many nice things,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,4107,1386,108"
