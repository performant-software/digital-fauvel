      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Each one with her shield at her neck.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,2022,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "There is no man, however much he has lived,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,2125,1281,103"
    },
    
    {
