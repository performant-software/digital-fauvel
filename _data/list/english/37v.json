      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Each one prays that God will save",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2614,708,1260,131"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The women who ride such horses",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2614,839,1260,131"
