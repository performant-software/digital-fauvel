      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,2919,1386,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Joust to go to the place",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,3027,1386,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Where the barriers were placed for this purpose.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=1197,3135,1386,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The Virtues, therefore, who had devoted",
