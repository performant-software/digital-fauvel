    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "This can be demonstrated through the sapphire,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,1122,1197,102"
    },
    
    {
