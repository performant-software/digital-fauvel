      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,5610,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
