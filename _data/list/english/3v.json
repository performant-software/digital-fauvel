{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/english/3v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel is not a reasonable animal",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,808,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He is all appearances and not stable;",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,905,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Full of deception, empty of truth,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1002,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A figure made for deceiving people.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1099,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel is truly an animal",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1196,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For otherwise he could not have",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1293,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sovereignty over other animals.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1390,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "One can know this through reason:",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1487,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The king of men is a man,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1584,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The eagle is the king of the birds just as",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1681,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The lion is the king of animals.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1778,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Know, therefore, you who are present,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1875,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That Fauvel, this noble lord,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,1972,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Is the sovereign and greatest",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2069,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Of all men who, like animals,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2166,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Put their hearts and heads in the world.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2263,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Such men we should call animals,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2360,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Of whom Fauvel is the king and lord.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2457,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He is the equal of a Templar heretic,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2554,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He who makes Fauvel his idol.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2651,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel is an animal, and he is an animal also",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2748,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Whoever honors and celebrates him.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2845,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Now I want to tell you and prove",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,2942,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Why Fauvel is devoid of reason:",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3039,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He should stay supine in his stable",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3136,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But for no amount of money would he do this",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3233,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For he does not want to stay in his place.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3330,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He does nothing but come and go.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3427,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In order to obtain a good manger.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3524,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He applies himself in order to please.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3621,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In trickery and falseness,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3718,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He has placed all of his intelligence.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3815,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He has come and gone so much",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,3912,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That he will never be held",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4009,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Or stopped by a bit or a bridle.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4106,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He races everywhere and runs and gallops.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4203,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He follows Fortune without reason",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4300,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And thus he reigns in every season.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4397,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In order to lift himself up, he applies himself hard",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4494,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In the instruction manual of flattery,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4591,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "So that he can make the fake shine brightly,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4688,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Render truth silent and take away what is right.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4785,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "His definition, discussed above,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4882,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "I have given; may it be enough for you.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,4979,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Now I will discuss his manner,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5076,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In continuing with my material,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5173,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "How and why Fauvel, in an abundant way,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5270,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel transforms the world into his pasture",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5367,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And how both the great and the small",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5464,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Are eager to rub him.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5561,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "God, the omnipotent king,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5658,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who knows all,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5755,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In the beginning when he made",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5852,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The world, with glory,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,5949,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He put each creature in its proper place",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,6046,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And gave it its own nature.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,6143,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He ordered everything so wisely,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,756,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That no man can, unless he lies,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,852,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pretend that he has any faultâ€”",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,948,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For no one possesses such a degree of mastery.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1044,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But what he had ordained",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1140,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Is so changed and poorly led",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1236,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That it is necessary, according to reason,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1332,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That we be near the moment",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1428,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "When the world will come to an end,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1524,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And God will make another world.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1620,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Reason makes a strong argument:",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1716,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "First of all, since, without any instrument,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1812,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "It pleased God to make the world",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1908,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And he wanted to depict out of clay",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2004,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mankind and mold him in his image.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2100,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To man he gave such an advantage,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2196,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That he made him the king of animals",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2292,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And the greatest in nobility;",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2388,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But now everything is all topsy-turvy,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2484,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That which God had prepared,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2580,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For men have turned into animals.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,3822,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "They hold their heads close to the ground:",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,3920,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nothing interests them except owning land,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4018,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "They do not wish to know anything about God.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4116,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nor do they ever look towards the sky",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4214,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For they burn in the fire of covetousness.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4312,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The operation demonstrates the argument that",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4410,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Form is shaped in the same way",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4508,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "As the work.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4606,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For this reason, I say certainly",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4704,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That men have truly become animals",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4802,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "When they live in the manner of animals.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4900,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And for this reason we can rightly say",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4998,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That Fauvel is the lord of the world",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5096,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For he is honored everywhere",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5194,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And adored as a God on Earth.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5292,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Reason has lost her sovereignty",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5390,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "When we see bestiality",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5488,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Placed so high above men.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5586,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And falsity breaking reason.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5684,1166,98"
    }
    
  ]
}
