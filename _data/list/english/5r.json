  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Humanity might come onto the path",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,703,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Through which to reach eternal joy.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,800,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "These apostles had disciples",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,897,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who received, under their guidance, the power",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,994,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To link everything and unlink it",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1091,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And to reconcile us with God.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1188,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The apostles became bishops",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1285,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For they principally received responsibility",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1382,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For governing the Church.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1479,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And when faith was in its beginnings,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1576,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Disciples spread themselves",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1673,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Around the world and worked",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1770,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To multiply the faith,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1867,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And they thought to apply themselves",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,1964,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "So that they won over many souls",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2061,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who became ladies in Paradise.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2158,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Saint Peter, who was a pope,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2255,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Did not dress himself in precious cloth,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2352,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nor did he live from exactions.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2449,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "His only consolation",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2546,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Was to live as a saint",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2643,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And to act so that the world would be delivered",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2740,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "From the slavery of sin",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2837,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And that it would be set on the course of good.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,2934,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And when he had worked",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3031,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And prayed to God for his people,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3128,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He did not care for a life of begging.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3225,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rather, in order to obtain food,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3322,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He lived from fishing",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3419,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And thereby gained a modest life.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3516,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But, know this: he did not know how to fish.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3613,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He owned a small net",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3710,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And a small boat,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3807,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And only caught a few minnows",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,3904,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And other small fish.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4001,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And in this way his life was sustained.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4098,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But our current pope",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4195,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fishes in a much better spot.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4292,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He has a large and strong fishing line",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4389,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Which brings him florins of gold",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4486,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "So that Saint Peter and his boat",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4583,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tremble and are tossed about.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4680,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He fishes for innumerable florins.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4777,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "He does not think about how the shadow",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4874,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Of Saint Peter used to cure",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,4971,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Anyone who complained of any pain.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5068,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Saint Peter lived without wealth,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5165,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "But now his successor has such a profusion of it",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5262,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That it is a great marvel.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5359,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For Fauvel applies himself and makes sure",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5456,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To bring him many florins,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/5r#xywh=1659,5553,1218,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
