{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/10r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la faute de Fauvel, cette funeste bête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,766,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par laquelle nous tombe dessus toute tempête,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,875,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puisqu'il pervertit le monde de la sorte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,984,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Partout, sans mesure et sans limite,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1093,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que dans ce contexte toute créature",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1202,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A abandonné la nature qui lui était propre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1311,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et s'est changée en son contraire, de la manière",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1420,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que j'ai décrite ci-dessus, sommairement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1529,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "- Car j'y mettrais trop de temps",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1638,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si je développais tout en détail -,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1747,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je conclus en bonne logique",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1856,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que nous sommes proches du moment",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,1965,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Où doit arriver la fin du monde,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2074,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car toutes les formes de méchanceté y surabondent.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2183,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'ailleurs la prophétie nous dit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2292,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que lorsque la bonne foi viendra à manquer sur terre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2401,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce sera le moment où se produira l'avènement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2510,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Du fils de Dieu au jugement dernier ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2619,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et je constate que la bonne foi disparaît au point",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2728,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il est fort possible qu'elle quitte la terre :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2837,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si jamais il reste encore un peu de bonne foi, elle est morte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2946,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la foi sans les œuvres n'a aucune force.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3055,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi, je suis persuadé que l'Antéchrist est venu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3164,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car Cupidon règne ainsi que Vénus,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3273,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En tous lieux, parmi les grands et les petits,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3382,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et je ne connais ni homme ni femme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3491,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui en vérité ne puisse affirmer à juste titre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3600,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que plus il vit en ce monde, pire il devient.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3709,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est là le fait de Fauvel l'enragé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3818,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui si souvent bénéficie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,3927,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'aide de Maugis et de Basin :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4036,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il mène le monde entier par antiphrase",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4145,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est à dire au contraire de ce qu'il faudrait.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4254,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout un chacun devrait s'en tenir bien éloigné",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4363,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car dans sa personne et dans son visage",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4472,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nous voyons comme le portrait et la figuration",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4581,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De toute espèce de fausseté et de flatterie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4690,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de l'idolâtrie généralisée ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4799,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout le monde est aujourd'hui à la même école :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,4908,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Être aux petits soins pour Fauvel, l'idole;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5017,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On manifeste un si grand respect",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5126,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Envers chacun de ceux qui ont obtenu l'excellence",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5235,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En matière de situation, de richesse ou de puissance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5344,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que je n'hésite pas à affirmer que, pour cette raison,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5453,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On ne témoigne pas aujourd'hui",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5562,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À Dieu l'honneur qui lui est dû.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5671,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On dépense aujourd'hui plus d'efforts",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5780,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour servir la créature humaine,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5889,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin d'obtenir la richesse temporelle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5998,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'à servir celui qui fait largesse",
