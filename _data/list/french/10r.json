      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au nom de Dieu, qu'il me soit pardonné !",
