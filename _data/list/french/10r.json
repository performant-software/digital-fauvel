      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que je l'ai touché trop près de la loupe,",
