      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas de prélat, de clerc ou de prêtre",
