      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À me servir et à être aux petits soins pour moi ;",
