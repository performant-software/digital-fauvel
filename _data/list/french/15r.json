    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il tenait en main un petit bâton de sureau.",
