      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,3097,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous voyez que le monde entier aspire",
