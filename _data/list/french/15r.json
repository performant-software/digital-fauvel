      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2047,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne donnait pas l'impression d'être enrhumé,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2152,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais parlait fort ; voici le thème de son discours :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2257,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Seigneurs et dames qui êtes présents ici,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2362,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous devez manifester votre joie et faire la fête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2467,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car vous faites partie de ma maison",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2572,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et appartenez tous à mon lignage ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2677,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et vous constatez que notre situation",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2782,1239,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/15r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est si prospère que cela doit vous faire bien plaisir ;",
