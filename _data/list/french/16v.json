{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/16v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Néanmoins, à l'intérieur sont cachées",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,693,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De petites émeraudes vertes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,795,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui font l'effet de petits grains de sel,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,897,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien que personne ne s'aperçoit de leur présence",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,999,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sauf les malheureux qui la portent,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1101,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui y trouvent du réconfort dans leur affliction.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1203,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Voilà les deux couronnes que tenait en main",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1305,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fortune, à qui cela allait parfaitement.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1407,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais son visage était tout à fait hideux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1509,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et je ne connais personne, proche ou étranger,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1611,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui l'ait aussi affreux que Fortune !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1713,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un des côtés est sombre et de teinte brune,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1815,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout à fait hideuse à regarder,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,1917,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tandis que l'autre est belle et gracieuse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2019,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Délicate, blanche, avec un teint éclatant et vermeil.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2121,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne donne pas l'impression d'être une femme triste",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2223,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand on la regarde de ce côté-là ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2325,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'autre, elle a l'air d'un léopard,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2427,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Semblant pleine de fureur, cruelle et féroce,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2529,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et toujours prête à frapper.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2631,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un de ses yeux est rouge et brûlant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2733,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cruel, et son regard est horrible ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2835,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est celui qui se trouve sur la moitié gauche;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,2937,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En revanche, elle a l'œil droit brillant et enjoué",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3039,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'ils sont séduits et piégés,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3141,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qu'elle regarde avec l'œil droit.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3243,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle avait revêtu une robe partagée en deux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3345,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fortune, avec une partie faite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3447,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De drap de soie moiré, et non d'étoffe de laine,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3549,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'elle prend diverses couleurs",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3651,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'autre était fabriquée dans un vieux tissu de serge",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3753,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont Fortune est généreuse et libérale",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3855,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car elle en distribue des pans et des morceaux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,3957,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À quiconque va de ce côté-là.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4059,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y avait deux roues devant Fortune",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4161,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui tournaient sans arrêt, mais l'une",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4263,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Va vite et l'autre doucement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4365,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et dans chacune, en vérité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4467,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Était placée une roue plus petite,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4569,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui se trouvait à l'intérieur, et de telle façon",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4671,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elle tournait en sens inverse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4773,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la roue dans laquelle elle était installée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4875,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ces roues ne s'arrêtent pas",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,4977,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De faire tourner l'état du monde :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5079,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le jeu auquel Fortune se divertit.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5181,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle cajole les uns, aux autres elle distribue des coups,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5283,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car à chaque tour qu'elle fait faire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5385,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle en anéantit certains et en rétablit d'autres ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5487,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À quelques-uns elle offre sa couronne,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5589,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puis la reprend, et la redonne,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5691,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une fois la mauvaise, l'autre la bonne ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5793,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est ainsi qu'elle répartit la joie et la peine",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5895,1260,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tant et si bien qu'aucun humain ne peut savoir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,714,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle part de mal ou de bien il peut obtenir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,817,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car Fortune est prompte à jeter au vent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,920,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De tous côtés, ses volontés.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1023,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À ses pieds était assise Vaine Gloire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1126,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui n'avait qu'une seule chose en tête,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1229,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est de se parer avec grâce",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1332,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour faire en sorte qu'ils se contemplent en elle, comme dans un miroir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1435,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui sont en train de monter au sommet des roues ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1538,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par ses nobles apprêts elle les encourage",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1641,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À lui porter toute leur attention et à la regarder",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1744,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De telle façon qu'ils ne songent point à la chute,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1847,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui les attend quand les roues font un tour",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,1950,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au moment où Fortune y donne un coup de patte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3843,2053,1218,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Voilà dans quel état se présentait Fortune",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3402,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand Fauvel avec son assemblée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3506,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Arriva à Macrocosme.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3610,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il savait que Fortune y était, et alla immédiatement la trouver,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3714,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et, au spectacle de sa majesté,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3818,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il marqua un arrêt à quelque distance :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,3922,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rien d'étonnant à ce qu'il ait eu peur!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4026,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Néanmoins, il prit les bonnes dispositions",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4130,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour parler de manière avisée :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4234,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était agenouillé très humblement.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4338,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme quelqu'un qui connaît bien son affaire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4442,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il salua Fortune, la noble dame,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4546,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se mit ensuite à sa besogne",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4650,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En s'exprimant exactement en ces termes:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4754,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Ma dame, que le souverain Seigneur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4858,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui n'a pas d'égal ni de supérieur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,4962,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous accorde longue vie et joie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5066,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qu'il vous garde en votre éminente position !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5170,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le grand désir qui s'imposait à moi",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5274,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De vous voir m'a fait me mettre en route",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5378,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et donné le courage de venir ici ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5482,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je n'arrivais plus à résister !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5586,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "J'aurais passé pour insensé ou indolent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5690,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si je n'avais eu le désir ardent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5794,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De vous voir, ma chère dame,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,5898,1260,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/16v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À qui j'offre ici mon corps et mon âme.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=3874,6002,1260,104"
    }
    
  ]
}
