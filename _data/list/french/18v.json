      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/18v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des honneurs que j'ai obtenus à votre ombre !",
