  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À cause de Fauvel, que je vois torcher avec tant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,1827,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout en douceur, sans qu'on l'égratigne,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,1924,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "J'ai sombré dans la mélancolie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2021,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parce qu'il est aujourd'hui une bête au poil si brillant.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2118,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Souvent le voient représenté en peinture",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2215,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des gens qui ne savent pas si ce qui est figuré par lui",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2312,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est dérision, sagesse ou folie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2409,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est pourquoi, sans la moindre équivoque,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2506,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je vais dire sur ce genre de bête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2603,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce qui peut me passer par la tête.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2700,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel ne couche plus dans une écurie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2797,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il possède une demeure plus honorable :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2894,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il exige une mangeoire plus prestigieuse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,2991,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un beau râtelier et une nourriture abondante.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3088,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a pris ses quartiers dans la salle d'apparat,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3185,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin de mieux exhiber sa dignité royale.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3282,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais à travers les autres pièces aussi,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3379,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Grâce à son savoir, on le révère beaucoup,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3476,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et il arrive fréquemment que dans les chambres",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3573,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il fasse rassembler toute sa suite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3670,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui avec tant d'application le frotte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3767,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il ne peut subsister la moindre crotte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3864,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fortune, l'adversaire de Raison,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,3961,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En a fait le seigneur de sa maison :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4058,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle se donne beaucoup de mal pour le promouvoir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4155,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puisqu'elle l'emmène au palais du roi;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4252,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne cesse de lui faire obtenir des honneurs.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4349,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Autour de Fauvel, il y a une telle foule qui se presse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4446,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De gens issus de toutes nations",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4543,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de toutes conditions",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4640,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que c'en est tout à fait extraordinaire :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4737,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et pas un qui ne se mette en quatre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4834,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour bouchonner Fauvel tout en douceur ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,4931,1124,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le rassemblement en est prodigieux !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1r#xywh=1690,5028,1124,97"
    }
    
  ]
