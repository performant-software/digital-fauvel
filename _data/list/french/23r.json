{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/23r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "«Et qui ne veut et d'ailleurs ne mérite pas",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,735,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que les honnêtes gens disent du bien de lui.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,840,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est ainsi que celui qui est monté haut,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,945,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "soit plein de vice ou de qualités,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1050,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Subit toutes sortes de contrariétés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1155,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui viennent de la vérité ou de l'envie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1260,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou d'un grand chagrin ou de l'inquiétude,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1365,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la maladie ou du mauvais sort ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1470,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et c'est ce que veut dire la petite roue",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1575,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui s'oppose constamment à la grande:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1680,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'y a personne, en effet, c'est incontestable,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1785,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À qui Fortune se donne tout entière,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1890,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car même si elle se donnait complètement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,1995,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle est par ailleurs si extraordinairement capricieuse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2100,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elle voudrait repartir aussitôt",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2205,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que nul ne pourrait la retenir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2310,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "«Jamais on n'a vu sur terre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2415,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un homme à ce point favorisé de la fortune",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2520,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui n'ait en même temps que son bonheur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2625,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une part non négligeable d'adversité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2730,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme par exemple quelqu'un qui possède",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2835,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'argent, qui en a plein ses coffres et ses greniers,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,2940,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais qui n'a que peu d'amis,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3045,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui est un rustre, un bâtard, ou qui a voué",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3150,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son cœur et son corps à la convoitise,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3255,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au que chacun n'a pour lui que haine et mépris,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3360,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qut met une telle passion à posséder",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3465,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il ne supporte pas de manger à satiété,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3570,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou alors, il acquiert sa fortune de telle manière",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3675,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que c'est au prix de sa damnation.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3780,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Un autre exemple : un homme qui est noble,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3885,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Courtois, libéral et soucieux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,3990,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'honorer les gens de bien,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4095,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais qui est chez lui dans un tel dénuement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4200,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que rien n'est aussi froid que son âtre.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4305,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'un côté je suis pour lui une marâtre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4410,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de l'autre je suis comme sa mère:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4515,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est ainsi que sa situation est entremêlée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4620,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est honoré pour sa noblesse de caractère",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4725,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais la pauvreté le touche au plus profond de lui-même ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4830,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il préférerait être moine cloîtré,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,4935,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Là où personne ne pourrait le reconnaître !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5040,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Encore un exemple : un homme beau, avec de belles rentes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5145,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et une femme selon ses désirs,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5250,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais qui ne peut pas avoir d'enfants",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5355,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne sait pas qui va hériter de ses biens,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5460,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tandis qu'il y en a d'aucuns, par mon âme,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5565,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ont de beaux enfants et une belle femme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5670,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais en entend rapporter souvent des nouvelles",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5775,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui pour lui sont tristes et cruelles.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5880,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas de prince, de roi ou d'empereur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,5985,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle que soit la hauteur du trône où il est assis,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=1543,6090,1145,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle que soit sa ou l'étendue de sa puissance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,703,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour qui tout se prodmt selon ses désirs,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,809,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la petite roue à contresens",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,915,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fait vite se noyer celui qui nage.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1021,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et cette même finalité peut se trouver",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1127,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans la grande roue qui tourne lentement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1233,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui signifie l'homme qui s'élève",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1339,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou qui tombe, connaissant la douleur et la honte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1445,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que celui-ci prenne garde, c'est le conseil que je lui",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1551,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'autre roue plus petite tournant à contresens, donne,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1657,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ne cesse d'opposer sa résistance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1763,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour enlever clous ou rivets et faire chuter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1869,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelqu'un de la roue du dessus :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,1975,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le jeu de la \"chantepleure \" !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2081,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Par ces exemples tu peux avoir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2187,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une idée pour me connaître.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2293,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tu n'as pas perdu tes efforts",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2399,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si tu çomprends bien ce qu'est Fortune !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2505,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si tu savais parfaitement de quoi il s'agit,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2611,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tu n'aurais vraiment pas beaucoup d'amour pour le monde",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2717,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et tu n'y chercherais pas ton bonheur,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2823,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car rien n'y est assuré.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2929,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle sécurité ont les gens riches",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3035,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui possèdent des biens en grandes quantités ?",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3141,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leurs richesses leur sont très vite enlevées :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3247,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La mort leur touche de près les flancs",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3353,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De telle manière qu'ils savent avec certitude",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3459,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'ils ne vivront pas longtemps,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3565,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car dès qu'un enfant est sorti du ventre maternel,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3671,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne fait rien d'autre que mourir et avancer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3777,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vers sa mort d'heure en heure;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3883,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'ailleurs, aussitôt né, il pleure",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,3989,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et donne l'impression que Nature lui dit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4095,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il naît à une existence pénible et vouée à la mort,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4201,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans laquelle il chemine en hâtant le pas :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4307,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne fera qu'un bref passage",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4413,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Connaissant grand tourment et grande peine,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4519,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont la fin n'est jamais certaine.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4625,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi nourrissent-ils un désir tout à fait insensé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4731,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui mettent leur plaisir suprême",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4837,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans la possession des richesses temporelles :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,4943,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils s'imaginent qu'ils sont au comble du bonheur,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,5049,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Etre favorisés par la fortune et chanceux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,5155,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand leurs biens sont plantureux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,5261,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais je t'affirme en vérité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,5367,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que ce n'est pas une forme de félicité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,5473,1271,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/23r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que la possession des richesses temporelles,",
        "language": "french"
      },
