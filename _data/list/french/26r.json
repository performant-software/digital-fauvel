{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/26r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'homme dont Amour gouverne le cœur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,367,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Blessé et enflammé d'un ardent désir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,475,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En voyant que le sort lui est contraire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,583,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Du côté de la dame à qui il fait le service d'amour,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,691,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Même s'il réussit, par un moyen quelconque,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,799,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A trouver en lui la consolation de ses tourments ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,907,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'il y arrive, il faut s'émerveiller",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1015,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De ce qu'Amour soit capable de mettre en si mauvaise posture",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1123,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celui qu'il emprisonne dans ses liens :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1231,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ille fait dormir bien peu, et beaucoup veiller,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1339,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Prendre peu de repos, et beaucoup de peine ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1447,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout son esprit est encerclé et étreint",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1555,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ensuite par le désir, qu'apporte l'amour.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1663,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il bouleverse le cœur d'une ardeur si vive",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1771,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la pensée, qu'il se consume comme une mèche",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1879,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A force de désirer l'honneur suprême",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1987,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour l'acquisition duquel les amants ont bien de la peine :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2095,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le désir consume le cœur et dessèche le corps.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2203,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et lorsqu'un homme en arrive à ce point",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2311,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'en permanencè son corps est dans la peine,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2419,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son cœur se consume. tout entier, son esprit est captif",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2527,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et il est frappé par les mauvais coups du sort !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2635,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'un homme dans cette situation ne peut manquer d'être hors de son sens,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2743,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si je l'affirme, je ne me trompe en rien.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2851,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est bien vrai qu'il peut, en douce espérance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2959,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Trouver un soulagement de ses tourments,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3067,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'amant en qui il y a du cœur,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3175,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais quand il se rend compte que le malheur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3283,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pèse plus lourd dans sa balance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3391,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne lui reste guère d'espoir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3499,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas ! moi qui vous débite ces paroles,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3607,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je suis déchiré nuit et jour par un tel combat!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3715,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Amour ne me laisse aucun repos :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3823,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il emprisonne mon esprit, il fait souffrir mon corps ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,3931,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le désir attise sans cesse à nouveau mon cœur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4039,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'une ardeur qui ne s'arrête pas en moi.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4147,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De pâtir tourments du corps, sans mentir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4255,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou de ressentir la brûlure du cœur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4363,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas! je n'oserais pas me plaindre à ce point,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4471,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si le sort voulait bien consentir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4579,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin de me protéger dans mes [souffrances],",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4687,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A ce qu'en moi l'espoir pût demeurer fort.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4795,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais quand je me vois à ce point infortuné",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,4903,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que je vois la chance s'élancer loin de moi",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5011,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au moment où la brûlure d'amour",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5119,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vient si férocement attaquer mon cœur,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5227,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est compréhensible qu'en moi tout bon espoir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5335,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Doit complètement disparaître, en vérité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5443,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Assurément, il est bien évident que la chance me fuit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5551,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que je suis poursuivi par toutes les malchances",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5659,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puisque je me rends compte qu'à ma dame",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5767,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que je chéris tant, il lui pèse et l'ennuie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,5875,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le tourment que subis pour elle jour et nuit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,556,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que sa mine ne me laisse espérer de secours.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,660,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est-il donc, dès lors, extraordinaire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,764,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si, en sentant que mon corps, mon cœur et mon esprit sont affligés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,868,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par les souffrances que je sens venir de l'amour",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,972,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et en ne voyant aucune.bienveillance du sort",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1076,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni de la part de celle qui n'a pas d'égale,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1180,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que je veuille en finir, dans le désespoir?",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1284,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il vaut mieux que je consente à ma fin",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1388,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Plutôt que de subir en une continuelle langueur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1492,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et sans le réconfort de l'espérance, le martyre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1596,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À quelqu'un qui s'attend à la mort avec certitude :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1700,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mieux vaut pour lui une mort rapide qu'une lente ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1804,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Entre deux maux, mieux vaut le moindre !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,1908,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas ! mais si mon cœur se soumet à cette perspective,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2012,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La raison me blâme et me donne",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2116,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le conseil d'abandonner cette voie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2220,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car il est trop méprisable, l'individu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2324,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui se livre à une mort si ignominieuse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2428,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui fourvoie son esprit en fin de compte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2532,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mon dieu, que faire ? Me voilà pris dans un dilemme !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2636,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la raison je suis critiqué sans ménagements",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2740,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si le désespoir me fait aspirer à la mort ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2844,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'autre part, je suis accablé de douleur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,2948,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au point de ne plus accorder beaucoup de valeur à ma vie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3052,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car je ne crois guère pouvoir m'attendre à la grâce.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3156,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne peux pas, vraiment, rester dans une telle situation!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3260,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que vais-je donc faire? C'est en me lamentant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3364,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur mes tourments que je veux, si possible, me distraire ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3468,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je dis pas par là que je vais les diminuer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3572,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car ce résultat, je ne peux l'atteindre sans celle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3676,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui pourrait rapidement m'en réconforter.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3780,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Réconforter? Hélas ! j'ai vu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3884,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Depuis longtemps qu'elle s'en moquait",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,3988,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et cela m'a donné la tentation du désespoir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4092,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car elle savait parfaitement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4196,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que c'est vers elle que j'ai et que j'avais",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4300,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tourné mon cœur et ma volonté.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4404,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toutefois, quoi qu'il arrive,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4508,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Soit qu'elle prenne pitié de moi ou qu'elle s'en moque,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4612,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je veux, à celle que j'aime sans tricher",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4716,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Adresser ma complainte, sans faute :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4820,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que Dieu m'accorde qu'elle serve à soulager",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,4924,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les cruels tourments que je reçois en réponse.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,5028,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et puisque les choses en sont arrivées là,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,5132,1397,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que j'en suis venu à me plaindre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=2824,5236,1397,104"
    },
    
    {
