      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si vous ne daignez pas vous disposer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5349,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À me soigner de mes maux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5455,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par le tendre secours qui viendrait de vous.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5561,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Assurément, si je n'ai pas votre tendre secours,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5667,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ma chère dame, je cours au plus vite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5773,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la mort, et je suis débarrassé de mes maux.",
