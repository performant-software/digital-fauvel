      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,4925,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne peux trouver d'autre issue",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5031,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À mes cruels tourments, que la mort,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5137,1376,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chère dame, qui m'attend à coup sûr",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/26v#xywh=3769,5243,1376,106"
    },
    
    {
