  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il nomme ceux qui vont jouter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,556,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et leur montre tout ce qu'ils auront à faire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,662,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En leur disant de préparer immédiatement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,768,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout ce qui leur est nécessaire, et d'y veiller,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,874,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et leur demande à chacun de se comporter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,980,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De façon à ne pas avoir honte de la joute.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,1086,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils partent aussitôt vaquer à la besogne",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,1192,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'ayant aucune raison d'attendre plus longtemps.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,1298,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur tout ce qui se passe là, les informations",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,1404,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parviennent aux dames, qui chantent ce verset :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1197,1510,1197,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/33v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et après avoir sollicité le secours et l'aide",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/33v#xywh=1228,2761,1208,124"
    },
    
    {
