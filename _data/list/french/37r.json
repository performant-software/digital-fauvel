{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/37r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il s'agissait de ceux que Fauvel avait",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,672,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fait veiller, afin de les préparer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,772,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la joute, et qui se trouvaient là",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,872,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur le champ, tous déjà prêts, et qui essayaient",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,972,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leurs grands chevaux et leurs destriers.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1072,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À les voir, ils étaient si farouches",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1172,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que je les tins pour fous furieux :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1272,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était manifeste que c'est le diable",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1372,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui les avait amenés en cette enceinte !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1472,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cela se passait les prés devant l'église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1572,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De Saint-Germain, dans une plaine",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1672,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Située entre les murs de la ville et la Seine.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,1772,1186,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est avec plaisir que j'admirais les lices :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3097,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles n'avaient pas été faites avec du bois de fente",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3195,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais constituées de gros et solides morceaux de bois de construction;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3293,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et comme je savais parfaitement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3391,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Où logeaient les Vertus, je descendis par les prés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3489,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la Seine, et par un bac je me rendis",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3587,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Devant le logis en question ; là, j'ai vu,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3685,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si du moins je n'ai pas été enlevé dans les airs,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3783,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un spectacle merveilleux, tel qu'il n'y en eut jamais :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3881,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au-dessus du logis je vis, en effet, deux échelles",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,3979,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont les extrémités inférieures s'accrochaient",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,4077,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux pignons, si nous en avons bien pris la mesure.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=147,4175,1155,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quant aux extrémités supérieures, elles touchaient le ciel,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,5502,1186,118"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et sur les échelles allaient et venaient",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,5620,1186,118"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A mon avis des anges et des archanges.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,5738,1186,118"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne le dis pas pour vous raconter de belles histoires",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=126,5856,1186,118"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais pour vous rapporter le miracle.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,630,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À travers les rues il y avait une telle presse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,729,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour voir grimper les anges",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,828,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que c'est incroyable à relater.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,927,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je crois qu'ils venaient apporter leur réconfort",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,1026,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux dames, et puis qu'ils repartaient.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1459,1125,1092,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les anges chantaient, comme il me semble,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1417,5281,1040,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette prose, tous en chœur :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37r#xywh=1417,5407,1040,126"
    }
    
  ]
}
