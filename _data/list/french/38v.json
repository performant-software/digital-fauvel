      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3762,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec un semis de fleurs de lis dorées,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3865,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'autres de couleur rouge avec des lions",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3968,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rampants en blanc : il n'existe point ici d'homme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4071,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Capable de les rappeler en totalité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4174,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni de les décrire sans éveiller les doutes.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4277,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Virginité poursuit et conduit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4380,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette tâche souverainement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4483,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et c'est elle qui est la mieux parée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4586,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et la plus gracieusement armée :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4689,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle portait des armes blanches et sans tache,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4792,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Brillamment dorées et peintes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4895,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'images de très beaux petits angelots.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,4998,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jamais Tristan ou Lancelot",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,5101,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'avaient eu droit à de plus belles parures !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,5204,1302,103"
