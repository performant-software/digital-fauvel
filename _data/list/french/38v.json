      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le prix de son noble équipage.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,5410,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
