      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3144,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui portaient des armes de couleur brune",
        "language": "french"
