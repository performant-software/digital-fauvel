    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On aurait eu du mal à évaluer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,5307,1302,103"
