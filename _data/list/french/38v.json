      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3247,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec le bord entaillé de pointes d'argent et d'or.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3350,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand je rêve dans mon sommeil, j'ai l'impression",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3453,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir leur noble contenance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3556,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la bannière que je tiens dans ma main!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3659,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y en avait d'autres de couleur bleue",
