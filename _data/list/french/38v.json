      "on": "http://fauvel.archivengine.com/canvas/38v#xywh=3906,3041,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/38v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y en avait parmi elles quelques-unes",
        "language": "french"
