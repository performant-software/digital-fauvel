      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,5292,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Raison a perdu sa souveraineté",
