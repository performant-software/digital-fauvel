      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,756,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'aucun être humain ne peut, sauf à mentir,",
