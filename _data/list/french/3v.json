      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,948,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car personne ne possède un tel degré de maîtrise.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,1044,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais ce qu'il avait mis en ordre",
