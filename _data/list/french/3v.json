      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=1102,6143,1260,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a tout mis en ordre avec tant de sagesse",
