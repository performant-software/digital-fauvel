      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,2388,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais aujourd'hui elle est complètement sens dessus dessous",
