      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3769,4900,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et pour cela nous pouvons dire à juste titre",
