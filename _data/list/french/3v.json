      },
      "on": "http://fauvel.archivengine.com/canvas/3v#xywh=3738,852,1155,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Prétendre qu'il ait quelque défaut",
        "language": "french"
