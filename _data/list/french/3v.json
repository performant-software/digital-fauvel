    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est à ce point bouleversé et malmené",
