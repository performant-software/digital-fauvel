  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'ayant repérée, elle se prépare à l'assaut",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3339,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tandis que Fornication se met en branle :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3442,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chacune des deux lace les courroies du heaume",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3545,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et elles se mettent en place pour jouter ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3648,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chasteté se saisit de sa lance et la fixe,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3751,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et, l'écu suspendu au cou, que cela plaise ou non,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3854,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a touché son adversaire d'un coup tel",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,3957,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que celle-ci aurait perdu la vie sur le champ",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4060,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si n'avait été dame Destinée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4163,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui a prolongé son existence ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4266,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cependant, elle tomba complètement à la renverse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4369,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Devant les yeux de la troupe fauveline.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4472,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Humilité, l'amie de Dieu,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4575,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne se trouvait pas très loin des lices;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4678,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle était fort richement équipée :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4781,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ses parures étaient tout en blanc",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4884,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ornées de motifs et façonnées",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,4987,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la même manière que celles de sa mère Virginité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5090,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Impossible, pour dire la vérité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5193,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De décrire la capacité de dame",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5296,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Humilité et sa valeur morale",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5399,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour réduire à néant l'emportement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5502,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'Orgueil; d'ailleurs, Outrecuidance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5605,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Manifeste le vif désir et l'intention",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5708,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'affronter, mais Abstinence",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5811,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et Pitié ont déjà arrangé la chose :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=168,5914,1386,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles vont demander à Humilité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3360,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce don, en récompense",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3463,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour la demoiselle Patience,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3566,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et obtenir qu'elle ait la permission et le droit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3669,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aller aussitôt jouter contre Orgueil,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3772,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En priant qu'au nom de Dieu, elle n'échoue pas dans l'entreprise.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3875,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Humilité, à cause de leur requête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,3978,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et pour la raison, aussi, que c'est Patience",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4081,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui portait sa bannière en la précédant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4184,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'a octroyé publiquement.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4287,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle n'accepterait en échange la richesse de Bourgueil,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4390,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Patience, qui a l'intention, sans orgueil,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4493,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'entreprendre une joute contre Orgueil.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4596,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne cherche à se mesurer à nul autre.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4699,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle portait des armes semblables à celle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4802,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De sa maîtresse. Au milieu de la foule",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,4905,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a repéré Orgueil, entouré d'une nombreuse troupe.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5008,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En toute une journée, je n'arriverais pas",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5111,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À décrire ses équipements précieux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5214,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je dirai juste qu'ils sont riches et ostentatoires.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5317,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chacun des adversaires connaissait les intentions de l'autre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5420,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi prirent-ils directement le chemin le long des rangées.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5523,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les assistants faisaient l'éloge, tous ensemble,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5626,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De Patience, car c'était avec humilité",
