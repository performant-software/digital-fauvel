  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Seigneur Dieu, père spirituel,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,1984,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout-puissant, sage et véritable,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2086,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toi qui vis au sein de la sainte Trinité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2188,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans une seule et même divinité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2290,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toi qui as créé la Terre à partir du néant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2392,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi que la mer, et le ciel et tout ce qu'il entoure,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2494,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toi qui as fait l'homme à ta ressemblance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2596,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui lui as donné la sagesse et le pouvoir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2698,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'obtenir la félicité parfaite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2800,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et la loi que tu lui as réservée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2902,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'il veut bien la suivre loyalement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3004,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Seigneur, toi qui récompenses généreusement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3106,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chacun selon ses mérites,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3208,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En gain ou en perte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3310,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toi qui as tant aimé le genre humain",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3412,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que pour le délivrer de l'esclavage",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3514,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Où le tenait le démon et de la mort",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3616,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'enfer, dont les victimes ont été innombrables,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3718,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tu as envoyé ton Fils, ta propre sagesse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3820,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui forme avec toi une seule essence,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3922,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec toi et avec le Saint-Espirit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4024,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme l'explique l'Ecriture,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4126,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Grâce à qui nos péchés sont plus légers,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4228,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tu l'as envoyé depuis ton trône souverain",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4330,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et as fait incarner",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4432,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sa divinité dans notre humanité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4534,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au sein de la Vierge, mystère ineffable !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4636,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans que la Vierge toute pure",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4738,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En subisse de souillure",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4840,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni dans son corps, ni dans son âme ni dans sa pensée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4942,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est d'elle qu'il est né à Bethléem;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,5044,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est là que Dieu fut réuni avec l'homme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,5146,1229,102"
    }
    
  ]
}
