{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/5v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il lui envoie le produit de ses décimes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,661,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et lui offre des prébendes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,757,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour ses clercs, partout et avec libéralité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,853,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas ! quelle attitude déplorable,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,949,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car dans cette situation nous voyons l'Église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1045,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Payer le tribut et se trouver en grande difficulté,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1141,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puisque Fauvel, qui est connaisseur en la matière,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1237,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A obtenu les décimes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1333,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour le roi, en s'adressant au pape.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1429,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans cette affaire, le pape ne porte pas sa cape",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1525,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et n'est pas le tuteur du clergé,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1621,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais il est transformé en exécutant du roi,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1717,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien que c'est de la justice laïque",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1813,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que relève sainte Église.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,1909,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La chose est délibérée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2005,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Donc il ne sert à rien de protester.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2101,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi le pape frotte Fauvel",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2197,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'il en écorche le clergé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2293,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans pourtant poser la main sur lui, en apparence,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2389,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais la sainte Eglise en tremble de tous ses membres.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,2485,1123,96"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que pouvons-nous dire des cardinaux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4011,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans risquer en quoi que ce soit de les mettre en colère ?",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4109,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sauf tout le respect qu'on leur doit,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4207,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nous pouvons porter ce jugement:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4305,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'aux premiers apôtres ils ressemblent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4403,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Peu, me semble-t-il ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4501,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les premiers apôtres étaient pauvres",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4599,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et vivaient petitement :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4697,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils étaient pleins de charité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4795,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et fondaient leur existence sur l'humilité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4893,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Maintenant les dés ont roulé, hélas !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,4991,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car c'est une autre voie que prennent nos prélats.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5089,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi ne puis-je raconter là-dessus une bonne histoire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5187,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car Avarice et Vaine Gloire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5285,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les ont arrachés au droit chemin,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5383,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se tiennent trop près à côté d'eux.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5481,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il sont pasteurs, mais c'est pour se repaître eux-mêmes :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5579,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aujourd'hui le loup est le maître des brebis !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5677,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils savent parfaitement leur enlever la laine du dos",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5775,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si près de la peau qu'elle saigne ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5873,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il sont tous tout prêts à tondre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,5971,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et n'attendent pas jusqu'à l'été",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=1197,6069,1207,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais en toute saison ils plument et pillent ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,672,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est Fauvel qui le veut, car ils l'étrillent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,773,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et le frottent tout en douceur.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,874,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils chantent Placebo à haute voix",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,975,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais murmurent dirige sans faire entendre de note.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1076,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est un tremblement de terre généralisé,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1177,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la terre tourne en sens inverse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1278,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et clercs et laïcs, moines et citadins",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1379,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sont tournés sens dessus dessous.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1480,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas, comment se fait-il que soient installés en chaire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1581,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la simonie, de jeunes prélats",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1682,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui n'ont aucune espèce d'instruction :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1783,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils portent le nom de révérend père",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1884,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne sont que des enfants : cela ne doit pas faire plaisir à Dieu,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,1985,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir que l'Église son épouse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2086,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se trouve placée entre les mains de tels prélats.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2187,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce ne sont pas des dirigeants à la main sûre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2288,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les jeunes gens manquant de maturité !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2389,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cependant, à bien voir les choses avec recul,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2490,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne saurais distinguer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2591,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle espèce de prélats, à dire vrai,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2692,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Accomplit au jour d'aujourd'hui au mieux ses devoirs :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2793,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les uns, j'en parlerai encore,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2894,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Participent au conseil du roi :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,2995,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux enquêtes, aux jugements,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3096,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux cours de justice, aux parlements",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3197,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vont assister nos prélats, et ils savent bien y faire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3298,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils y dilapident les biens de l'Église :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3399,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les pauvres en reçoivent une part minime",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3500,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car c'est Fauvel qui répartit les aumônes,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3601,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont il prodigue la totalité pour le faste",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3702,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En ne donnant rien aux pauvres gens.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3803,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par la faute des prélats de cet acabit, comme je le crois,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,3904,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'Église se trouve aujourd'hui plus démunie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4005,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De foi qu'on ne l'a jamais vu être auparavant.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4106,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est sur leurs conseils que le roi est souvent amené",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4207,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À lever des impôts vexatoires",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4308,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur l'Église et à l'accabler.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4409,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À cause des prélats qui veulent plaire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4510,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au roi et faire tout son bon plaisir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4611,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La sainte Église est aujourd'hui déchue",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4712,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et perd aujourd'hui sa liberté.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4813,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les prélats de cette espèce ne sont pas des pères,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,4914,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais des parâtres qui n'aiment guère",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5015,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les enfants de celles qu'ils ont épousées",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5116,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et par qui les honneurs leur sont attribués :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5217,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les enfants ne peuvent pas leur plaire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5318,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont ils n'aiment pas la mère.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5419,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nos prélats remplissent bien leurs devoirs,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5520,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car ils sont des poteaux de sureau",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5621,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de roseau dans la sainte Eglise,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5722,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ils la soutiennent de telle façon",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5823,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'ils tenaillent et taillent leurs sujets",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,5924,1312,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/5v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et manquent à tous leurs besoins.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/5v#xywh=3759,6025,1312,101"
    }
    
  ]
}
