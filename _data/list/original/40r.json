      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4173,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tel coup li donna que secours",
