      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4989,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Abstinence veult, mais el n'ose;",
