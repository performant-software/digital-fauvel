      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4275,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne li vaut hui mes une nate!",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4377,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si fu feru par la gargate,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4479,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'en cheant a la terre mole,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4581,1460,102"
