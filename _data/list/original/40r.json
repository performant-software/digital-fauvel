    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Du col se rompi la chaignole.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4683,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "PPar raison et par jugement",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4785,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ont les dames [fait] mandement",
