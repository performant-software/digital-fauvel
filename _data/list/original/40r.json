      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5832,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mes Orgueil si fort se desroie",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=1554,5935,1323,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que il semble que tout soit sien.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,619,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Patience si set moult bien",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,745,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que par souffrir a l'en victoire",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,871,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais que l'en ait en sa memoire",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,997,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De prendre le meilleur du jeu",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,1123,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quant en vient en temps et en lieu.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,1249,1292,126"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "OOrgueil n'entent que Vainne Gloire.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,2541,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pacience, ce devez croire,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,2643,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A tel chose ne pense pas :",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,2745,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son destrier fait plus que le pas",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,2847,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Passer pour soi mettre a son point ;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,2949,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son heaume, qu'ele ot de bon point,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3051,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lace et son escu embrace ;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3153,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sa lance empoingne, puis la face",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3255,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tourne devers son adversier.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3357,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un court a l'autre de cuer fier :",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3459,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A l'encontrer leur lances rompent;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3561,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aresnes sonnent, trompeurs trompent",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3663,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et revient chascun a sa route.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3765,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Patience point ne redoute",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3867,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Orgueil ne tretout son bobant :",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,3969,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne le va pas lobant,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4071,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car de sa lance a l'autre cours",
