    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Forment se moutre tout le val,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,5703,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A Abstinence faire paour",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,5805,1460,102"
