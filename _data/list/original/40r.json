    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Congié a eü de cueur lié :",
