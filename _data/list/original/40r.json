      },
      "on": "http://fauvel.archivengine.com/canvas/40r#xywh=2803,4887,1460,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Patience qu'el se repose.",
