    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Emprendre a courre sanz congié.",
