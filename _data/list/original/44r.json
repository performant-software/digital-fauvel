      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui tenebres efface et oste",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,846,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et toute occurté enlumine",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,947,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par les rais de sa clarté fine.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1048,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hostes ert, et droit a la lettre,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1149,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui voust oster et non pas mettre ;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1250,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Enfer en lui moult mal hoste a",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1351,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car toute sa gent en osta",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1452,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les conduit droit a la voie",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1553,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En son paradis, plain de joie,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1654,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou touz jourz vivront en leesce",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1755,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sanz mal souffrir et sanz tristesce.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1856,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Beau douz Pere, plain de pitié,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,1957,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui tant amas virginité",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2058,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que la Vierge mere saintisme,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2159,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quant pendis en la croiz hautime",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2260,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ou tu souffris mort angoisseuse,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2361,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour garder aussi com t'espouse",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2462,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Commandas a Jehan vierge apostre;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2563,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si com c'est la creance nostre",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2664,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que ce soit voirs, fais nous venjance",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2765,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De Fauvel qui de toute France",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2866,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Met le fruit a perdicïon.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,2967,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se tu n'i mez provisïon,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3068,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par lui feront tuit le retour",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3169,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En enfer : hé ! Diex ! de ce tour",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3270,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nous voilliez garder et deffendre,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3371,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais fais y sanz respit descendre",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3472,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel et sa mesnie toute,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3573,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car il boit le sanc et transgloute",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3674,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De ta plaie qu'as rachetee",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3775,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De ton precïeus sanc ! donnee",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3876,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "T'est la querele, qu'ele est teue",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,3977,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se il ainsi de toi se jeue",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4078,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que n'en soutiengne ta cause.",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4179,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je meintendrai sànz faire pause",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4280,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que s'il ainsi demeure quittes",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4381,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que ne li rendes ses merites,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4482,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ton pueple s'en esbahira",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/44r#xywh=283,4583,1229,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/44r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de pis en pis s'en yra.",
