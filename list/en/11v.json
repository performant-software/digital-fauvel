      "motivation":"sc:painting",
      "resource": {
        "@id": "11v-en-1344-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Draw their glory and arrogance from them.",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/11v#xywh=1102,5132,1208,107"
    },
    
    {
      "@id": "11v-en-1345",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "11v-en-1345-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "With this kind of crown was decorated",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/11v#xywh=1102,5239,1208,107"
    },
    
