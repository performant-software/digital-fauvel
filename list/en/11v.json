      "motivation":"sc:painting",
      "resource": {
        "@id": "11v-en-1343-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who are rich and full of vanity",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/11v#xywh=1102,5025,1208,107"
    },
    
    {
      "@id": "11v-en-1344",
