      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2293,1271,106"
    },
    
    {
      "@id": "23r-en-2809",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2809-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "You have not wasted your efforts",
