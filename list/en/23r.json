      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2399,1271,106"
    },
    
    {
      "@id": "23r-en-2810",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2810-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "If you understand well what is Fortune!",
