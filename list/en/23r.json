      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2505,1271,106"
    },
    
    {
      "@id": "23r-en-2811",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2811-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "If you knew well what is good,",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2611,1271,106"
    },
    
    {
      "@id": "23r-en-2812",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2812-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "You would have little love for the world",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2717,1271,106"
    },
    
    {
      "@id": "23r-en-2813",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2813-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And would not seek your good luck there,",
        "language": "en"
