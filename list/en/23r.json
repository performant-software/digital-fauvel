    },
    
    {
      "@id": "23r-en-2808",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2808-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
