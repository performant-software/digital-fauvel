      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2823,1271,106"
    },
    
    {
      "@id": "23r-en-2814",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2814-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For nothing there is certain.",
