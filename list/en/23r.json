      },
      "on": "http://fauvel.archivengine.com/canvas/23r#xywh=2824,2929,1271,106"
    },
    
    {
      "@id": "23r-en-2815",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-en-2815-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "What certainty do rich men",
