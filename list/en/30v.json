      "motivation":"sc:painting",
      "resource": {
        "@id": "30v-en-4175-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "There is no city so renowned",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/30v#xywh=3780,3531,1333,103"
