        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2128,1281,102"
    },
    
    {
      "@id": "34r-en-4827",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-en-4827-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And they will remain close the whole night,",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2230,1281,102"
    },
    
    {
      "@id": "34r-en-4828",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-en-4828-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
