      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4943-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Along with the siskin [?] and goldfish",
