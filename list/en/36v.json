      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4967-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The night and the power that belongs to it",
        "language": "en"
