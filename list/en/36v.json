      "resource": {
        "@id": "36v-en-4970-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "I went along, very happy,",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/36v#xywh=3895,5425,1271,102"
    },
    
    {
      "@id": "36v-en-4971",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4971-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To the place where the assembly was to occur",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/36v#xywh=3895,5527,1271,102"
    },
    
    {
      "@id": "36v-en-4972",
