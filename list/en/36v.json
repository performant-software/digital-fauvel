      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4945-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Announce the day and perceive the dawn.",
        "language": "en"
      },
      "on": "http://fauvel.archivengine.com/canvas/36v#xywh=3895,2875,1271,102"
    },
    
    {
      "@id": "36v-en-4946",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4946-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "It is in this time that the hawthorn",
