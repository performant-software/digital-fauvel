      "on": "http://fauvel.archivengine.com/canvas/36v#xywh=3927,2254,1260,100"
    },
    
    {
      "@id": "36v-en-4941",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4941-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "I would like to return to the joust",
