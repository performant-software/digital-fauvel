      "on": "http://fauvel.archivengine.com/canvas/36v#xywh=3895,2977,1271,102"
    },
    
    {
      "@id": "36v-en-4947",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "36v-en-4947-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And the flowers rejoice,",
        "language": "en"
