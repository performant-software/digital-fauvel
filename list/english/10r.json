      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2837,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "If there is a little faith, it is expiring,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2946,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "For faith without deeds is not strong.",
