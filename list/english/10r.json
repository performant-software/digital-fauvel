      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2292,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That when faith will be absent from the world,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2401,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Then will come",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,2510,1123,109"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The son of God, in the Last Judgment;",
