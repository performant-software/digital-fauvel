    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/10r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And I do not know any woman or man,",
        "language": "english"
