    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "If luck had wished to grant me",
