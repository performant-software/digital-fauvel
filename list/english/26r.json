      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And when man is brought to this point,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2311,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That at all times his body is pained,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,2419,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "His heart completely burning, his intelligence captured,",
