      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1879,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
