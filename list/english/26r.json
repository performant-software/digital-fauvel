      },
      "on": "http://fauvel.archivengine.com/canvas/26r#xywh=1491,1123,1333,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/26r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "The man who is entangled in his trap.",
