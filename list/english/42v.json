    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/42v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "With his poisonous decision",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/42v#xywh=2499,834,1207,110"
