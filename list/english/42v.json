    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/42v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "So that it seems, and it is true,",
