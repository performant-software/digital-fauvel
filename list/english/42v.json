      "on": "http://fauvel.archivengine.com/canvas/42v#xywh=1029,2754,1396,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/42v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That they do not destroy!",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/42v#xywh=1029,2856,1396,102"
    },
    
    {
