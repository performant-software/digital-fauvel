      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/42v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And which comes from the sons and daughters",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/42v#xywh=1029,2346,1396,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/42v",
        "@type": "dctypes:Text",
        "format": "text/plain",
