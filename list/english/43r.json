{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/english/43r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lord God, spiritual father,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,1984,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Omnipotent, wise and true,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2086,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who abides in the holy Trinity",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2188,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In one selfsame divinity,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2290,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who made the earth from nothing,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2392,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sea and sky and whatever it encompasses,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2494,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who made man in your image",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2596,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And gave him sense and power",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2698,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To obtain perfect joy",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2800,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And the law that you gave him,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,2902,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "If he wants to observe it loyally,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3004,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lord who generously rewards",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3106,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Each person according to his merits,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3208,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Whether to his gain or loss,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3310,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who so loved the human race",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3412,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "That you freed it from slavery",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3514,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "To the enemy and the death",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3616,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Of hell, which has put many to death,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3718,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "From your sovereign throne,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3820,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "You sent your son, your own wisdom,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,3922,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Who with you is one essence,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4024,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And the Holy Spirit,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4126,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "As Scripture explains,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4228,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Through which our sins are lightened,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4330,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "And made his divinity",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4432,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dress itself in our humanity,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4534,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Through the Virgin, ineffably,",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4636,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Without the pure Virgin",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4738,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sustaining any corruption",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4840,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "In the body, soul or thought.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,4942,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "From her was he born in Bethlehem.",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,5044,1229,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/43r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "There was God joined to man",
        "language": "english"
      },
      "on": "http://fauvel.archivengine.com/canvas/43r#xywh=1543,5146,1229,102"
    }
    
  ]
}
