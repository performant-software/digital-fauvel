        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/10r#xywh=315,5671,1123,109"
    },
    
    {
      "@id": "10r-fr-1205",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "10r-fr-1205-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
