      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "11v-fr-1362-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Même s'ils possèdent un solide bon sens,",
