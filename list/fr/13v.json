      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1599-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toute proche d'elle il y avait Gloutonnerie",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3853,5533,1187,113"
    },
    
    {
      "@id": "13v-fr-1600",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1600-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont le teint était assombri et terni à force de trop manger.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3853,5646,1187,113"
    },
    
    {
      "@id": "13v-fr-1601",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1601-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puis Ivresse et Débauche",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3853,5759,1187,113"
    },
    
    {
      "@id": "13v-fr-1602",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1602-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui à aucun moment, de toute leur existence,",
        "language": "fr"
