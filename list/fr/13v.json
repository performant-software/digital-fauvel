      "resource": {
        "@id": "13v-fr-1594-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui attrape les gens, qu'ils soient vêtus ou nus :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3822,3508,1165,108"
    },
    
    {
      "@id": "13v-fr-1595",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1595-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le péché naturel dont le goût plaît",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3822,3616,1165,108"
    },
    
    {
      "@id": "13v-fr-1596",
