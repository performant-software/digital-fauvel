      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1596-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toujours, comme celui du calville blanc,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3822,3724,1165,108"
    },
    
    {
      "@id": "13v-fr-1597",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1597-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tant que la vieillesse, la maladie ou la sagesse",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3822,3832,1165,108"
    },
    
    {
      "@id": "13v-fr-1598",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "13v-fr-1598-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne l'ont fait passer de saison.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/13v#xywh=3822,3940,1165,108"
    },
    
    {
      "@id": "13v-fr-1599",
