      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1711-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais parlait fort ; voici le thème de son discours :",
