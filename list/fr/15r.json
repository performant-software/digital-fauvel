      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1727-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'ils ne m'étrillent pas ou me frottent ou me cajolent.",
