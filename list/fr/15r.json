    },
    
    {
      "@id": "15r-fr-1725",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1725-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Frottent mieux Fauvel que Morel !",
