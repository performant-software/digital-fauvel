      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1710-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne donnait pas l'impression d'être enrhumé,",
