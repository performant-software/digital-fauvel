      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,3307,1239,105"
    },
    
    {
      "@id": "15r-fr-1722",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1722-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas de prélat, de clerc ou de prêtre",
