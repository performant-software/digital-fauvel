      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1733-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui mènent à la situation éminente où je me suis hissé.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,4567,1239,105"
