      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1734-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je n'ai pas emprunté la droite voie,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,4672,1239,105"
