      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2992,1239,105"
    },
    
    {
      "@id": "15r-fr-1719",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1719-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi bien du royaume que de l'empire;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,3097,1239,105"
    },
    
    {
      "@id": "15r-fr-1720",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1720-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous voyez que le monde entier aspire",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,3202,1239,105"
    },
    
    {
      "@id": "15r-fr-1721",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1721-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À me servir et à être aux petits soins pour moi ;",
