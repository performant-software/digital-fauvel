      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,3412,1239,105"
    },
    
    {
      "@id": "15r-fr-1723",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1723-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ne souhaite faire partie de mes gens,",
