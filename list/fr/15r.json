    {
      "@id": "15r-fr-1724",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1724-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et tous les princes temporels",
