      "resource": {
        "@id": "15r-fr-1736-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La droite voie m'est trop pénible,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,4882,1239,105"
    },
    
    {
      "@id": "15r-fr-1737",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1737-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Alors que la tortueuse me paraît efficace et rapide !",
