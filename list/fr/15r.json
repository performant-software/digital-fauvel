      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1726-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous n'en, voyez pas beaucoup qui aient de beaux habits",
