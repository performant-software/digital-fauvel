      },
      "on": "http://fauvel.archivengine.com/canvas/15r#xywh=1564,2887,1239,105"
    },
    
    {
      "@id": "15r-fr-1718",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "15r-fr-1718-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je suis, au jour d'aujourd'hui, roi et seigneur",
