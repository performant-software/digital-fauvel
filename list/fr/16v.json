      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1981-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Puis la reprend, et la redonne,",
