      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1984-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tant et si bien qu'aucun humain ne peut savoir",
