    {
      "@id": "16v-fr-1983",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1983-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est ainsi qu'elle répartit la joie et la peine",
