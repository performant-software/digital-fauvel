    },
    
    {
      "@id": "16v-fr-1982",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1982-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une fois la mauvaise, l'autre la bonne ;",
