        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle en anéantit certains et en rétablit d'autres ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/16v#xywh=1249,5487,1260,102"
    },
    
    {
      "@id": "16v-fr-1980",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1980-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À quelques-uns elle offre sa couronne,",
