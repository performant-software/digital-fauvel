      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1990-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est de se parer avec grâce",
