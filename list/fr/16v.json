    },
    
    {
      "@id": "16v-fr-1986",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "16v-fr-1986-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car Fortune est prompte à jeter au vent",
