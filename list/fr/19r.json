      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "19r-fr-2107-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je vous prie de m'accorder votre amour et le requiers:",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/19r#xywh=357,1136,1165,103"
    },
    
    {
      "@id": "19r-fr-2108",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "19r-fr-2108-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne désire rien de plus en ce monde;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/19r#xywh=357,1239,1165,103"
    },
    
    {
      "@id": "19r-fr-2109",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "19r-fr-2109-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et afin que vous n'imaginiez pas que je recherche",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/19r#xywh=357,1342,1165,103"
    },
    
    {
      "@id": "19r-fr-2110",
