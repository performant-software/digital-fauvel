      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23r-fr-2821-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car dès qu'un enfant est sorti du ventre maternel,",
