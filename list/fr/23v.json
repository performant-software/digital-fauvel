      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5755,1249,107"
    },
    
    {
      "@id": "23v-fr-2894",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2894-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En tournant leur donnera quelque faveur.",
