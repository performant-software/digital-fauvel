{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/fr/23v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@id": "23v-fr-2845",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2845-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En la richesse ne rend pas parfaits",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,619,1249,107"
    },
    
    {
      "@id": "23v-fr-2846",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2846-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les riches dans cette existence mortelle,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,726,1249,107"
    },
    
    {
      "@id": "23v-fr-2847",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2847-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la richesse fait facilement défaut",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,833,1249,107"
    },
    
    {
      "@id": "23v-fr-2848",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2848-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les riches ont d'ailleurs de nombreux handicaps,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,940,1249,107"
    },
    
    {
      "@id": "23v-fr-2849",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2849-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme par exemple pour la santé ou la joie",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1047,1249,107"
    },
    
    {
      "@id": "23v-fr-2850",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2850-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'argent ne peut pas leur donner.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1154,1249,107"
    },
    
    {
      "@id": "23v-fr-2851",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2851-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'autre part, les riches, dans plusieurs pays,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1261,1249,107"
    },
    
    {
      "@id": "23v-fr-2852",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2852-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sont en butte à la haine, à cause de leur richesse",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1368,1249,107"
    },
    
    {
      "@id": "23v-fr-2853",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2853-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car ceux qui distribuent leurs biens sont beaucoup plus",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1475,1249,107"
    },
    
    {
      "@id": "23v-fr-2854",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2854-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aimés que ceux qui les cachent.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1582,1249,107"
    },
    
    {
      "@id": "23v-fr-2855",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2855-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et Raison considère, à juste titre,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1689,1249,107"
    },
    
    {
      "@id": "23v-fr-2856",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2856-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il vit dans la servitude,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1796,1249,107"
    },
    
    {
      "@id": "23v-fr-2857",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2857-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la manière d'un serf et d'un infortuné,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,1903,1249,107"
    },
    
    {
      "@id": "23v-fr-2858",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2858-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celui qui, sur les biens qu'il a accumulés,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2010,1249,107"
    },
    
    {
      "@id": "23v-fr-2859",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2859-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'ose pas en donner, en toucher ou en prendre",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2117,1249,107"
    },
    
    {
      "@id": "23v-fr-2860",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2860-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni rien dépenser pour son propre besoin.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2224,1249,107"
    },
    
    {
      "@id": "23v-fr-2861",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2861-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y a peu de bonheur dans les richesses",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2331,1249,107"
    },
    
    {
      "@id": "23v-fr-2862",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2862-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sauf pour celui qui est suffisamment réfléchi",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2438,1249,107"
    },
    
    {
      "@id": "23v-fr-2863",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2863-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et rempli de bonnes dispositions",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2545,1249,107"
    },
    
    {
      "@id": "23v-fr-2864",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2864-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il en distribue aux pauvres à grand foison",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2652,1249,107"
    },
    
    {
      "@id": "23v-fr-2865",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2865-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui, par une piété sincère et authentique",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2759,1249,107"
    },
    
    {
      "@id": "23v-fr-2866",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2866-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les emploie en œuvres de charité,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2866,1249,107"
    },
    
    {
      "@id": "23v-fr-2867",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2867-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dépensant le reste, en sage",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,2973,1249,107"
    },
    
    {
      "@id": "23v-fr-2868",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2868-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour lui-même, pour de bons usages ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3080,1249,107"
    },
    
    {
      "@id": "23v-fr-2869",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2869-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Autrement, en effet, aucune richesse",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3187,1249,107"
    },
    
    {
      "@id": "23v-fr-2870",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2870-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'est autre chose que calamité et grande tristesse.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3294,1249,107"
    },
    
    {
      "@id": "23v-fr-2871",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2871-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Et toi, Fauvel, tu ne désires rien plus",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3401,1249,107"
    },
    
    {
      "@id": "23v-fr-2872",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2872-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que d'être, dans le monde, un grand seigneur,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3508,1249,107"
    },
    
    {
      "@id": "23v-fr-2873",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2873-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais sache bien que l'adversité",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3615,1249,107"
    },
    
    {
      "@id": "23v-fr-2874",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2874-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vaut mieux qu'une grande prospérité,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3722,1249,107"
    },
    
    {
      "@id": "23v-fr-2875",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2875-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la prospérité procure aux hommes l'ivresse",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3829,1249,107"
    },
    
    {
      "@id": "23v-fr-2876",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2876-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les fait vivre dans les jouissances,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,3936,1249,107"
    },
    
    {
      "@id": "23v-fr-2877",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2877-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle les caresse doucement et les flatte",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4043,1249,107"
    },
    
    {
      "@id": "23v-fr-2878",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2878-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jusqu'au moment où dame Fate leur tombe dessus",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4150,1249,107"
    },
    
    {
      "@id": "23v-fr-2879",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2879-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et retourne aussitôt leur situation, ignominieusement :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4257,1249,107"
    },
    
    {
      "@id": "23v-fr-2880",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2880-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est pour rien qu'ils s'étaient apprêtés ainsi!",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4364,1249,107"
    },
    
    {
      "@id": "23v-fr-2881",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2881-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne connaissaient jusque-là ni leurs amis",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4471,1249,107"
    },
    
    {
      "@id": "23v-fr-2882",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2882-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni leurs ennemis, mais quand elle les a mis",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4578,1249,107"
    },
    
    {
      "@id": "23v-fr-2883",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2883-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au plus bas, Fortune, et précipités à terre",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4685,1249,107"
    },
    
    {
      "@id": "23v-fr-2884",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2884-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De sorte qu'ils sont dans une misère noire,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4792,1249,107"
    },
    
    {
      "@id": "23v-fr-2885",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2885-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Alors ils découvrent ceux qui les aimaient",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,4899,1249,107"
    },
    
    {
      "@id": "23v-fr-2886",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2886-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et reconnaissent ceux qui les détestaient :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5006,1249,107"
    },
    
    {
      "@id": "23v-fr-2887",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2887-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En effet, pendant qu'ils possédaient une grande fortune,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5113,1249,107"
    },
    
    {
      "@id": "23v-fr-2888",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2888-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils n'auraient pas pu s'en rendre compte aussi bien;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5220,1249,107"
    },
    
    {
      "@id": "23v-fr-2889",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2889-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi est-ce là une vérité, et non une fable,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5327,1249,107"
    },
    
    {
      "@id": "23v-fr-2890",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2890-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'adversité est profitable,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5434,1249,107"
    },
    
    {
      "@id": "23v-fr-2891",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2891-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car les riches vivent dans la crainte",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5541,1249,107"
    },
    
    {
