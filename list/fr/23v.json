      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2625,3012,1102,111"
    },
    
    {
      "@id": "23v-fr-2917",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2917-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un riche de ce type, pauvre en esprit,",
        "language": "fr"
