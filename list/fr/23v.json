      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2897-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hormis celui à qui peuvent y suffire",
