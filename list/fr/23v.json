      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5862,1249,107"
    },
    
    {
      "@id": "23v-fr-2895",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2895-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est pourquoi, en ce monde hasardeux",
