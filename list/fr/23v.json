      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2926-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mon autorisation ! Que Dieu le garde ! Amen.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5180,1260,143"
    },
    
    {
      "@id": "23v-fr-2927",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2927-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Fauvel, je t'ai assez fait la leçon :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5323,1260,143"
    },
    
    {
      "@id": "23v-fr-2928",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2928-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que m'importe si cela t'a déplu ?",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5466,1260,143"
    },
    
    {
      "@id": "23v-fr-2929",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2929-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par mes propos, tu as l'avantage",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5609,1260,143"
    },
    
    {
      "@id": "23v-fr-2930",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2930-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De te rendre compte si tu es fou ou sage",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5752,1260,143"
    },
    
    {
      "@id": "23v-fr-2931",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2931-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de savoir quel genre de marchandises",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5895,1260,143"
    },
    
    {
      "@id": "23v-fr-2932",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2932-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vend Fortune, qui est l'objet de ton désir.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,6038,1260,143"
    },
    
    {
      "@id": "23v-fr-2933",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2933-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tu peux voir si c'est une grande dame",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=3937,630,1187,123"
    },
    
    {
      "@id": "23v-fr-2934",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2934-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que Fortune, que tu veux prendre pour épouse. »",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=3937,753,1187,123"
    },
    
    {
      "@id": "23v-fr-2935",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2935-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est alors que Fauvel a livré",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=3937,876,1187,123"
    },
    
    {
      "@id": "23v-fr-2936",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2936-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette balade, le cœur complètement retourné :",
        "language": "fr"
