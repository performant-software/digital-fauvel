      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2593,577,1155,108"
    },
    
    {
      "@id": "23v-fr-2898",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2898-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les biens du genre de ceux que Notre-Seigneur",
