      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=3937,999,1187,123"
    },
    
    {
      "@id": "23v-fr-2937",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2937-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En renouvelant sa complainte",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=3958,5008,1019,110"
