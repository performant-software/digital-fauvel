      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,4465,1260,143"
    },
    
    {
      "@id": "23v-fr-2922",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2922-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A fait la preuve, nette et claire,",
        "language": "fr"
