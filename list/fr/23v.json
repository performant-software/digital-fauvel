    },
    
    {
      "@id": "23v-fr-2919",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2919-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un clerc du roi de France, Gervais,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,4179,1260,143"
    },
    
    {
      "@id": "23v-fr-2920",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2920-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec les paroles qu'il a conçues",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,4322,1260,143"
    },
    
    {
      "@id": "23v-fr-2921",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2921-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans ce petit livre qu'il a composé,",
        "language": "fr"
