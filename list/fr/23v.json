      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2925-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans ce petit livre : n'allez pas me demander",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,5037,1260,143"
