      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=1197,5648,1249,107"
    },
    
    {
      "@id": "23v-fr-2893",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2893-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et croient toujours que Fortune",
