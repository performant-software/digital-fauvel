      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2625,3123,1102,111"
    },
    
    {
      "@id": "23v-fr-2918",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2918-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'Évangile le dit heureux.",
