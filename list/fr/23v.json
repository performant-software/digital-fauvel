      "motivation":"sc:painting",
      "resource": {
        "@id": "23v-fr-2924-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car il parle de façon tout à fait appropriée",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/23v#xywh=2583,4894,1260,143"
