      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "2r-fr-75-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais ne réussissent pas à être aussi près,",
