      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2944,1281,102"
    },
    
    {
      "@id": "34r-fr-4835",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4835-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans le lit, fort gracieusement.",
