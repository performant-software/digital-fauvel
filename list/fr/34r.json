      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4820-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais sur place ne se trouvait, pour tout dire,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1516,1281,102"
    },
    
    {
      "@id": "34r-fr-4821",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4821-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni dans la salle d'apparat ni dans tout l'édifice",
