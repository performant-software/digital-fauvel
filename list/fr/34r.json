      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4829-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Deux dames que j'estime peu,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2434,1281,102"
    },
    
    {
      "@id": "34r-fr-4830",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
