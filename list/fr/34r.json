    },
    
    {
      "@id": "34r-fr-4837",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4837-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
