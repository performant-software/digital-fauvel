      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4846-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aller se coucher : aussitôt, sans tarder,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,4977,1302,105"
    },
    
    {
