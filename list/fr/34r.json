    },
    
    {
      "@id": "34r-fr-4826",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4826-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
