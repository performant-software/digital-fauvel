    },
    
    {
      "@id": "34r-fr-4824",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4824-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour faire la bénédiction du lit nuptial,",
        "language": "fr"
