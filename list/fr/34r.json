      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3760,1281,102"
    },
    
    {
      "@id": "34r-fr-4843",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4843-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec humilité etsans protester,",
        "language": "fr"
