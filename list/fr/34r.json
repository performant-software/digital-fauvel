      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1924,1281,102"
    },
    
    {
      "@id": "34r-fr-4825",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4825-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En revanche tous les mauvais esprits sans exception",
