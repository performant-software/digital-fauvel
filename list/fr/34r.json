    },
    
    {
      "@id": "34r-fr-4823",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4823-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni d'eau bénite ni de bréviaire",
