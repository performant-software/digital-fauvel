        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas d'homme vivant sous le ciel",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5607,1302,105"
    },
    
    {
      "@id": "34r-fr-4853",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4853-text",
        "@type": "dctypes:Text",
