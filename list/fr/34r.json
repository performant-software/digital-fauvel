      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3862,1281,102"
    },
    
    {
      "@id": "34r-fr-4844",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4844-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car elle veut le préserver de la colère.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3964,1281,102"
    },
    
    {
      "@id": "34r-fr-4845",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4845-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel se dit qu'il est l'heure",
