    },
    
    {
      "@id": "34r-fr-4836",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4836-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
