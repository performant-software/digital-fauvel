    },
    
    {
      "@id": "34r-fr-4839",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4839-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel, qui l'a épousée :",
