      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4847-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il saute dans le lit pour coucher avec elle.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5082,1302,105"
    },
    
    {
      "@id": "34r-fr-4848",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4848-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cependant, jamais un charivari de cette ampleur",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5187,1302,105"
    },
    
    {
      "@id": "34r-fr-4849",
