      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3454,1281,102"
    },
    
    {
      "@id": "34r-fr-4840",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4840-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne bougera pas dans un sens ou l'autre pour l'éviter,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3556,1281,102"
    },
    
    {
      "@id": "34r-fr-4841",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4841-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais se montrera bienveillante",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3658,1281,102"
    },
    
    {
      "@id": "34r-fr-4842",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4842-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et subira tout ce qui doit lui arriver",
        "language": "fr"
