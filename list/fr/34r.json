      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5292,1302,105"
    },
    
    {
      "@id": "34r-fr-4850",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4850-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme celui que l'on pratique à travers les carrefours",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5397,1302,105"
    },
    
    {
      "@id": "34r-fr-4851",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4851-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la ville et à travers les rues.",
