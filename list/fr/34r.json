      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2638,1281,102"
    },
    
    {
      "@id": "34r-fr-4832",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4832-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui savent parfaitement dispenser de séduisantes paroles,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2740,1281,102"
    },
    
    {
      "@id": "34r-fr-4833",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4833-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ont dévêtu l'épouse",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2842,1281,102"
    },
    
    {
      "@id": "34r-fr-4834",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4834-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et l'ont couchée toute nue",
