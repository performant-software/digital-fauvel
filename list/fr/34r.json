      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4827-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et assisteront de près, pendant toute la nuit,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2230,1281,102"
    },
    
    {
      "@id": "34r-fr-4828",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4828-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À leur joie et à leurs ébattements.",
