      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34r-fr-4838-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lorsqu'il voudra venir la rejoindre,",
