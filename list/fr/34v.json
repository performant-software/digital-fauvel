      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4898-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'on n'aurait pas entendu Dieu tonner.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2927,1197,100"
    },
    
    {
      "@id": "34v-fr-4899",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4899-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils faisaient entendre, après; un vacarme de cris",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3027,1197,100"
    },
    
    {
      "@id": "34v-fr-4900",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4900-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tel que l'on n'en avait jamais entendu :",
