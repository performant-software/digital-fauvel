    {
      "@id": "34v-fr-4915",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4915-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était habillé d'un drap foncé de bonne qualité ;",
