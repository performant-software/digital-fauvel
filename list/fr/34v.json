      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3527,1197,100"
    },
    
    {
      "@id": "34v-fr-4905",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4905-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et un autre envoyait des excréments au visage des gens :",
