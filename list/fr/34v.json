      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3127,1197,100"
    },
    
    {
      "@id": "34v-fr-4901",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4901-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un montrait son cul au vent",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3227,1197,100"
    },
    
    {
      "@id": "34v-fr-4902",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4902-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'autre brisait un auvent",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3327,1197,100"
    },
    
    {
      "@id": "34v-fr-4903",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4903-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un autre encore cassait portes et fenêtres,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3427,1197,100"
    },
    
    {
      "@id": "34v-fr-4904",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4904-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un autre jetait du sel dans le puits,",
        "language": "fr"
