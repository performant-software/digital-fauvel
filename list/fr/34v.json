      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4913-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y avait avec eux un géant",
