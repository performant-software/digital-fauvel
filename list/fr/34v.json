      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4895-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles faisaient un bruit si violent et si inhabituel,",
