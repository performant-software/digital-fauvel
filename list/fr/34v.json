      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,4027,1197,100"
    },
    
    {
      "@id": "34v-fr-4910",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4910-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour chanter la chanson du diable !",
