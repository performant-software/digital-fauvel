    },
    
    {
      "@id": "34v-fr-4911",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4911-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un crie « corbeilles et vans »",
