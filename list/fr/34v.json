      "@id": "34v-fr-4917",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4917-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Accompagné de l'ensemble de son armée:",
