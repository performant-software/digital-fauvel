      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,835,1197,107"
    },
    
    {
      "@id": "34v-fr-4919",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4919-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était monté sur un cheval haut au garrot,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,942,1197,107"
    },
    
