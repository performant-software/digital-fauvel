      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4914-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui avançait en criant à tue-tête :",
