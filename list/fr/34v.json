      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4916-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je crois qu'il s'agissait d'Hellequin,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,621,1197,107"
