      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "34v-fr-4912-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et l'autre de quel côté souffle le vent.",
