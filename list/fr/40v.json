      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2142,1323,108"
    },
    
    {
      "@id": "40v-fr-5546",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5546-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Touche avec sa bannière si brutalement",
        "language": "fr"
