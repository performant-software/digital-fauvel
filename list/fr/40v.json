      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5548-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ce ne fut pas pour elle une plaisanterie,",
