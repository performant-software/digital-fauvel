      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2034,1323,108"
    },
    
    {
      "@id": "40v-fr-5545",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5545-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais Abstinence, au deuxième galop",
