    {
      "@id": "40v-fr-5552",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5552-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Abstinence ne reste pas collée sur place",
