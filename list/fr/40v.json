      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5538-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Gloutonnerie, avec des armoiries de couleur noire,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1386,1323,108"
