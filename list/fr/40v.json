      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5539-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rejoint sa rangée et saisit sa lance,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1494,1323,108"
    },
    
    {
      "@id": "40v-fr-5540",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5540-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et à ce moment, Abstinence s'avance.",
