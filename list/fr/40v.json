      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5547-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dame Gloutonnerie qu'elle en fut abrutie,",
