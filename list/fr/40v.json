      "motivation":"sc:painting",
      "resource": {
        "@id": "40v-fr-5554-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur Gloutonnerie, sans se soucier de qui s'en irrite,",
        "language": "fr"
