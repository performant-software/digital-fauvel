      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "5v-fr-581-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et n'est pas le tuteur du clergé,",
