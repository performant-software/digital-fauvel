      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-937-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui les a conduits à la mort, à leur perte,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3559,1239,107"
