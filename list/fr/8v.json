    {
      "@id": "8v-fr-950",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-950-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "M'ont fait subir outrage et vilenie.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,6255,1197,115"
    },
    
    {
      "@id": "8v-fr-951",
