      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-952-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et dressés dans la limite de mes possibilités",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,5827,1050,115"
