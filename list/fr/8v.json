    {
      "@id": "8v-fr-938",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-938-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En a fait une mère triste et affligée.",
