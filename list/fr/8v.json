      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-927-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La bête fauve a fait tant et si bien",
