      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-949-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui m'avaient témoigné tant d'honneur",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,6140,1197,115"
