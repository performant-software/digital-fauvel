      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-923-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aujourd'hui sous le froc et sous la robe !",
