      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-951-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Je les ai nourris et allaités",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,5712,1050,115"
