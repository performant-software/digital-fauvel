      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-947-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir que mes enfants m'ont délaissée !",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,5910,1197,115"
