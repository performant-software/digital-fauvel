      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-924-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par des gens de cette sorte Dieu est bien tourné en dérision :",
