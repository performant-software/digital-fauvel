      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-925-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le seigneur Fauvel qui est leur abbé",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2275,1239,107"
    },
    
