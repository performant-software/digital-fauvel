    {
      "@id": "8v-fr-948",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-948-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les Templiers que j'aimais tant",
