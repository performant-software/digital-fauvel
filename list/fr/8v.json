      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3880,1239,107"
    },
    
    {
      "@id": "8v-fr-941",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-941-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui est un véritable fils la sainte Église.",
