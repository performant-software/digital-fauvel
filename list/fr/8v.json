      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-935-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'en plaint amèrement et se lamente :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3345,1239,107"
    },
    
    {
      "@id": "8v-fr-936",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "8v-fr-936-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La tricherie des Templiers, bien avérée,",
