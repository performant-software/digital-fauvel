      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,827,1228,103"
    },
    
    {
      "@id": "9v-fr-1053",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1053-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
