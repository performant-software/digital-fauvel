      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,930,1228,103"
    },
    
    {
      "@id": "9v-fr-1054",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1054-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils veulent pourtant, en toute circonstance,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1033,1228,103"
    },
    
    {
      "@id": "9v-fr-1055",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1055-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'on leur déclare : «C'est bien dit, monseigneur!»",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1136,1228,103"
    },
    
    {
      "@id": "9v-fr-1056",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1056-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont contents quand on les considère comme sages.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1239,1228,103"
    },
    
    {
      "@id": "9v-fr-1057",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1057-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mon Dieu ! qu'il y a de visages faux",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1342,1228,103"
    },
    
    {
      "@id": "9v-fr-1058",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1058-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À travers tout le royaume et dans l'empire!",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1445,1228,103"
    },
    
    {
      "@id": "9v-fr-1059",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1059-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Faux sont les serviteurs, faux les seigneurs,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1548,1228,103"
    },
    
    {
      "@id": "9v-fr-1060",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1060-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Faux les petits, faux les plus grands,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1651,1228,103"
    },
    
    {
      "@id": "9v-fr-1061",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1061-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Faux, ils le sont aujourd'hui tous et toutes ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1754,1228,103"
    },
    
    {
      "@id": "9v-fr-1062",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1062-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les seigneurs ont leur pensée occupée à accabler",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1857,1228,103"
    },
    
    {
      "@id": "9v-fr-1063",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1063-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leurs sujets et à lever sur eux",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,1960,1228,103"
    },
    
    {
      "@id": "9v-fr-1064",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1064-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des impôts vexatoires et des taxes funestes.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2063,1228,103"
    },
    
    {
      "@id": "9v-fr-1065",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1065-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les grands seigneurs, c'est une chose tout à fait extraordinaire",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2166,1228,103"
    },
    
    {
      "@id": "9v-fr-1066",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1066-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne veulent pas qu'on leur donne des conseils",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2269,1228,103"
    },
    
    {
      "@id": "9v-fr-1067",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1067-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Autres que ceux qui conviennent le mieux à leurs idées,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2372,1228,103"
    },
    
    {
      "@id": "9v-fr-1068",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1068-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'ils sont nombreux, ceux qui n'osent plus dire la vérité",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2475,1228,103"
    },
    
    {
      "@id": "9v-fr-1069",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1069-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De peur de mettre en colère leur seigneur :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2578,1228,103"
    },
    
    {
      "@id": "9v-fr-1070",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1070-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est ainsi qu'ils frottent la bête fauve.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2681,1228,103"
    },
    
    {
      "@id": "9v-fr-1071",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1071-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'y a plus un seul roi, pas un duc, pas un comte,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2784,1228,103"
    },
    
    {
      "@id": "9v-fr-1072",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1072-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ne soit perverti et ne vive honteusement.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2887,1228,103"
    },
    
    {
      "@id": "9v-fr-1073",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1073-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il faudrait bien qu'ils s'inquiètent",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,2990,1228,103"
    },
    
    {
      "@id": "9v-fr-1074",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1074-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De ne pas œuvrer le droit et la justice :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3093,1228,103"
    },
    
    {
      "@id": "9v-fr-1075",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1075-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leur esprit est tout entier occupé par le désir",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3196,1228,103"
    },
    
    {
      "@id": "9v-fr-1076",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1076-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De s'enrichir sans rien payer en retour.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3299,1228,103"
    },
    
    {
      "@id": "9v-fr-1077",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1077-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les rois et les contes, autrefois, avaient coutume",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3402,1228,103"
    },
    
    {
      "@id": "9v-fr-1078",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1078-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De puiser largement dans les biens qu'ils possédaient",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3505,1228,103"
    },
    
    {
      "@id": "9v-fr-1079",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1079-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour fonder et doter les églises,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3608,1228,103"
    },
    
    {
      "@id": "9v-fr-1080",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1080-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mes ce sont eux, maintenant, qui les ruinent :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3711,1228,103"
    },
    
    {
      "@id": "9v-fr-1081",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1081-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils dépouillent et pillent le crucifix,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3814,1228,103"
    },
    
    {
      "@id": "9v-fr-1082",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1082-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et dérobent ses biens de les manières.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,3917,1228,103"
    },
    
    {
      "@id": "9v-fr-1083",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1083-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils n'ont que haine pour l'Église et tout le clergé ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4020,1228,103"
    },
    
    {
      "@id": "9v-fr-1084",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1084-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils se refusent à accomplir quelque bonne action que ce soit,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4123,1228,103"
    },
    
    {
      "@id": "9v-fr-1085",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1085-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi bien les chevaliers que les écuyers ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4226,1228,103"
    },
    
    {
      "@id": "9v-fr-1086",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1086-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont si imbus de leur noblesse aujourd'hui",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4329,1228,103"
    },
    
    {
      "@id": "9v-fr-1087",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1087-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'ils méprisent tous les autres,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4432,1228,103"
    },
    
    {
      "@id": "9v-fr-1088",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1088-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au point que cela ne peut qu'être très fâcheux.",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4535,1228,103"
    },
    
    {
      "@id": "9v-fr-1089",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1089-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les nobles sont pleins de vilenie,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4638,1228,103"
    },
    
    {
      "@id": "9v-fr-1090",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1090-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et c'est quelque chose de tout à fait antinomique",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4741,1228,103"
    },
    
    {
      "@id": "9v-fr-1091",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1091-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que la vilenie et la noblesse ;",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4844,1228,103"
    },
    
    {
      "@id": "9v-fr-1092",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1092-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et c'est pourquoi, il me semble en bonne logique",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,4947,1228,103"
    },
    
    {
      "@id": "9v-fr-1093",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1093-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que les deux ne peuvent être associées :",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,5050,1228,103"
    },
    
    {
      "@id": "9v-fr-1094",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1094-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La sagesse vaut mieux que la noblesse sans raison !",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,5153,1228,103"
    },
    
    {
      "@id": "9v-fr-1095",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1095-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La noblesse, comme dit le sage,",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,5256,1228,103"
    },
    
    {
      "@id": "9v-fr-1096",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1096-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne vient de rien d'autre que des qualités du cœur",
        "language": "fr"
      },
      "on": "http://fauvel.archivengine.com/canvas/9v#xywh=1155,5359,1228,103"
    },
    
    {
      "@id": "9v-fr-1097",
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "9v-fr-1097-text",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui est paré de bonnes mœurs :",
