      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'est marié et multiplié.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,2452,1176,111"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un jour, il séjournait en son palais,",
        "language": "french"
