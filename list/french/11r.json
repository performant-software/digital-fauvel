      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1585,3874,1166,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel, qui ne donne pas l'impression d'être un joyeux compagnon,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1585,3982,1166,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Bien qu'il possède au pays de Galles",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1585,4090,1166,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des châteaux, des donjons, des manoirs, des salles d'apparat.",
