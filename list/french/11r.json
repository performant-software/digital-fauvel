      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car il est le symbole de tout mal,",
        "language": "french"
