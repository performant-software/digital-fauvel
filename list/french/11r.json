      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et que, comme nous dit l'Écriture,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,1342,1176,111"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nul ne peut correctement éviter le vice",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,1453,1176,111"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'il n'a pas la connaissance préalable du mal,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,1564,1176,111"
    },
    
    {
