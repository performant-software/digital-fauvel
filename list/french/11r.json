      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,2008,1176,111"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin que nous puissions vivre débarrassés de lui",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,2119,1176,111"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
