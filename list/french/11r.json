      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/11r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de bien la garder en mémoire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/11r#xywh=1564,1120,1176,111"
    },
    
    {
