{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/1v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vous pourriez y voir des rois, des ducs et des comtes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,745,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Alliés pour bouchonner Fauvel ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,838,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tous les seigneurs temporels et les princes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,931,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Viennent à lui de toutes provinces,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1024,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi que les chevaliers, grands et petits,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1117,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui sont particulièrement habiles à frotter.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1210,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas, sachez-le, de roi ou de comte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1303,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À qui bouchonner Fauvel fasse éprouver de la honte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1396,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les vicomtes, les prévôts et les baillis",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1489,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne sont jamais en reste pour bien frotter;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1582,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les habitants des bourgs et des villes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1675,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le frottent avec une remarquable ingéniosité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1768,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De même que les paysans venus des villages",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1861,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se tiennent à côté de Fauvel pour le nourrir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1081,1954,1134,93"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ensuite, c'est en consistoire public",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,2992,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que se rend Fauvel, pourtant authentique animal,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3090,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et quand le pape voit une bête de la sorte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3188,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sachez qu'il lui fait fête avec beaucoup d'enthousiasme;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3286,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tandis que les cardinaux lui font grand honneur :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3384,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour le bouchonner ils accourent tous.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3482,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les vice-chanceliers, les notaires,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3580,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les auditeurs et les référendaires",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3678,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se mettent en frais et déploient tous leurs efforts",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3776,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour bouchonner Fauvel bien comme il faut.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3874,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les prélats n'épargnent pas les voyages par monts et par",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,3972,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour bouchonner ce noble cheval",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,4070,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les abbés, les religieux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,4168,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lui témoignent une extraordinaire dévotion.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=1102,4266,1166,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jacobins, Cordeliers, où êtes-vous ?",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,5470,1113,113"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette fois-ci, ne soyez pas comme des bêtes,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,5583,1113,113"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais songez à bien étudier la façon",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,5696,1113,113"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De caresser doucement Fauvel :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,5809,1113,113"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est bien ainsi que vous faites, que Dieu me vienne en aide,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,5922,1113,113"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/1v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au point que tout un chacun en est ébahi.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/1v#xywh=3696,6035,1113,113"
    }
    
  ]
}
