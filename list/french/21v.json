    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il en va ainsi de ces richesses",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2783,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de ces prestiges mondains",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2887,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dont l'apparence, en effet, est d'une extrême beauté,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2991,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais dont la nature véritable est telle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,3095,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elles sont vite dépensées,",
        "language": "french"
