      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,3719,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Partout, dans les puissants et les misérables,",
