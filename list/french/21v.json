    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui les possèdent, qui veillent sur elles",
