      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car en dedans les riches connaissent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4447,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Bien des misères dans leur cœur et dans leur tête.",
