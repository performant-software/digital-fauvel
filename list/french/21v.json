      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Appelle les richesses des épines,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4343,1176,104"
    },
    
    {
