      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2055,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ont placé en elles leur plaisir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2159,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leur jouissance et leur substance même,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,2263,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et pourtant, ces pierres ne sont pas",
