      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4655,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'ils n'ont pas fait l'expérience de la richesse !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4759,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La richesse, on l'acquiert avec peine,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4863,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On la possède ensuite dans l'anxiété",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,4967,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour l'abandonner avec une profonde tristesse.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,5071,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le cœur est en très grande détresse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/21v#xywh=1270,5175,1176,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/21v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une tristesse et une douleur extrêmes le saisissent",
