{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/2r.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les Augustins, les nonnes et les moines,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,1837,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les archidiacres et les chanoines",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,1942,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les clercs déjà pourvus par l'Église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2047,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se sont déjà bien mobilisés pour bouchonner,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2152,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les pauvres clercs qui n'ont pas de rente",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2257,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Y consacrent parfaitement toute leur attention",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2362,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais ne réussissent pas à être aussi près,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2467,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni à être autant devant que les grands personnages,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=409,2572,1229,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi en sont-ils fâchés et pleins de dépit,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,703,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au point qu'on les entend souvent dire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,801,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que Fortune, qui n'est pas stable",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,899,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui n'arrête pas de tourner,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,997,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Changera de place celui qui est le plus en avant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1095,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fera du haut le bas et de l'éloigné le proche.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1193,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le fait que Fortune tourne si rapidement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1291,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne met aucun terme à son mouvement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1389,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Apporte du réconfort aux pauvres",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1487,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les distrait agréablement de leurs malheurs.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1585,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et même quand ils ne réussissent pas à arriver",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1683,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jusqu'à bouchonner Fauvel et le toucher,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1781,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils disent : « Que Dieu garde une bête de cette sorte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1879,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a un fort beau corps et une fort belle tête. »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,1977,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il convient maintenant de savoir quelles sont la manière,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2075,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les comportements et la mine",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2173,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui sont adoptés pour bouchonner ce cheval fauve.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2271,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On ne reconnaît pas la nonne au voile ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2369,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est pourquoi il est bon qu'on les connaisse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2467,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car plusieurs le frottent qui n'y laissent de trace.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=1680,2565,1260,98"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je commencerai tout d'abord par le pape;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,1837,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'est pas juste qu'il soit épargné,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,1938,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais je déclare publiquement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2039,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que ce n'est pas mon intention",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2140,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De m'attaquer à l'honneur de Rome",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2241,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À qui doit obéir tout homme,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2342,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais de Fauvel je dirai la vérité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2443,1239,101"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi que je puis la concevoir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/2r#xywh=2971,2544,1239,101"
    }
    
  ]
}
