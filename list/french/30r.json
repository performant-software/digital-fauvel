  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« J'e les ferai dégringoler ignominieusement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,462,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ferai tuer les uns, pendre les autres,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,582,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les uns auront à subir des dommages et des pertes matérielles,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,702,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les autres mourront dans la misère !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,822,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je leur ferai à mainte reprise endurer mainte souffrance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,942,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quel que soit le déplaisir que cela te causera,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,1062,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De sorte que les gens de bien ressentiront une grande joie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,1182,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand les méchants suivront ce chemin. »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=189,1302,1291,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fàuvel a écouté et entendu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,514,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce que la dame lui a exposé:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,621,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est éperdu, ne sait plus que faire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,728,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ses larmes lui mouillent le visage ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,835,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne sachant plus quelle voie il doit prendre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,942,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni quelle doit être l'issue de l'affaire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,1049,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a fait le motet qui suit,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,1156,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais il n'y prend guère de plaisir!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=1564,1263,1176,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Entretemps Fortune réfléchit et se dit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,493,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que pour le moment elle ne veut plus accabler",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,597,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel, qui est frappé de stupeur, de plus d'outrages",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,701,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et elle lui tient ces propos : « Animal plein de perfidie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,805,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je ne veux pas, du moment que tu as franchi",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,909,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les portes de ma demeure, que tu partes sans avoir une faveur :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1013,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "«Voici la belle Vaine Gloire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1117,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La demoiselle pleine de tromperie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1221,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celle qui insidieusement enivre les gens",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1325,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui veulent vivre sur un grand pied,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1429,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui les conduit à se fier aux plaisirs",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1533,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin qu'ils s'oublient eux-mêmes et leur propre fin;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1637,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est elle qui jadis, pour les cinq vierges folles,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1741,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A éteint leurs lampes et leurs cierges,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1845,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la virginité qu'elles possédaient,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,1949,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles l'avaient entièrement soumise à Vaine Gloire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2053,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette demoiselle Vaine Gloire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2157,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui est aussi éphémère que transitoire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2261,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dispense des potions si douces à boire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2365,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "qu'elles font perdre toute faculté de penser",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2469,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la joie qui dure éternellement.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2573,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle d'une nature foncièrement trompeuse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2677,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vaine Gloire, tel est mon sentiment:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2781,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a très vite fait d'entraîner les gens",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2885,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vers les excès et la présomption,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,2989,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais elle réduit à néant tout bienfait,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,3093,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand elle se précipite pour l'accomplir, et le fait disparaître",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,3197,1396,104"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/30r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne supportant pas qu'il soit payé de retour.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/30r#xywh=2898,3301,1396,104"
    }
    
  ]
