  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Où se trouvait le lit, splendide et de noble allure :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,598,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'en existe de semblable jusqu'à Constantinople",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,700,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aussi décoré, d'aussi riche, d'aussi élégant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,802,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec sa couverture, sa courtepointe,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,904,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ses oreillers et ses tapis,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1006,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout cela orné de figures et de représentations",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1108,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'oiseaux, d'armes et de petites bêtes.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1210,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Toutes les images y sont si nettement reproduites",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1312,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que tout homme peut s'en émerveiller.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1414,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais sur place ne se trouvait, pour tout dire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1516,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni dans la salle d'apparat ni dans tout l'édifice",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1618,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pas de religieux, de clerc ou de prêtre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1720,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni d'eau bénite ni de bréviaire",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1822,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour faire la bénédiction du lit nuptial,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,1924,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En revanche tous les mauvais esprits sans exception",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2026,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Y sont servis et appréciés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2128,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et assisteront de près, pendant toute la nuit,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2230,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À leur joie et à leurs ébattements.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2332,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Deux dames que j'estime peu,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2434,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À savoir Séduction et Convoitise,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2536,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui sont les maîtresses des garde-robes,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2638,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui savent parfaitement dispenser de séduisantes paroles,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2740,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ont dévêtu l'épouse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2842,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et l'ont couchée toute nue",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,2944,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans le lit, fort gracieusement.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3046,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pas besoin de lui donner des leçons",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3148,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur la façon dont elle devra se comporter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3250,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lorsqu'il voudra venir la rejoindre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3352,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel, qui l'a épousée :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3454,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle ne bougera pas dans un sens ou l'autre pour l'éviter,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3556,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais se montrera bienveillante",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3658,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et subira tout ce qui doit lui arriver",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3760,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec humilité etsans protester,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3862,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car elle veut le préserver de la colère.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=220,3964,1281,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel se dit qu'il est l'heure",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,4872,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aller se coucher : aussitôt, sans tarder,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,4977,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il saute dans le lit pour coucher avec elle.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5082,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cependant, jamais un charivari de cette ampleur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5187,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'avait été jusque-là organisé par la canaille des fours",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5292,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme celui que l'on pratique à travers les carrefours",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5397,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la ville et à travers les rues.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5502,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'existe pas d'homme vivant sous le ciel",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5607,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui fût capable d'en faire une description,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5712,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quel que soit le talent qu'il possède.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=210,5817,1302,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux gui y participent se répandent partout :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=1596,546,1323,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils n'ont pas peur de Fauvel et de ses sbires;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=1596,661,1323,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils se sont placés sous bonne protection",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=1596,776,1323,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne se soucient pas de leurs ennemis.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=1596,891,1323,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la façon dont s'est déroulé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34r#xywh=2929,535,1103,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce charivari, et de sa forme,",
        "language": "french"
      },
