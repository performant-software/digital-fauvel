{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/34v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de la sorte ils faisaient avancer un chariot ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,1827,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À l'intérieur du chariot il y avait",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,1927,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un engin fait de roues de charrettes,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2027,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Solides, rigides et fort bien fabriquées,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2127,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui, en tournant,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2227,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Se heurtaient à six barres de fer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2327,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fixées à l'intérieur des moyeux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2427,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et bien attachées ; écoutez-moi bien :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2527,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles faisaient un bruit si violent et si inhabituel,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2627,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si horrible et si épouvantable",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2727,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elles produisaient au moment du choc,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2827,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'on n'aurait pas entendu Dieu tonner.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,2927,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils faisaient entendre, après; un vacarme de cris",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3027,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tel que l'on n'en avait jamais entendu :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3127,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un montrait son cul au vent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3227,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'autre brisait un auvent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3327,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un autre encore cassait portes et fenêtres,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3427,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un autre jetait du sel dans le puits,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3527,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et un autre envoyait des excréments au visage des gens :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3627,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'était une troupe vraiment affreuse et sauvage !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3727,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur leur tête, certains avaient des masques barbus ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3827,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec eux ils transportaient deux bières",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,3927,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans lesquelles se trouvaient des gens vraiment fàits",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,4027,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour chanter la chanson du diable !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,4127,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'un crie « corbeilles et vans »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,4227,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et l'autre de quel côté souffle le vent.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2509,4327,1197,100"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il y avait avec eux un géant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2625,5670,1113,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui avançait en criant à tue-tête :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=2625,5790,1113,120"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était habillé d'un drap foncé de bonne qualité ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,514,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je crois qu'il s'agissait d'Hellequin,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,621,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Accompagné de l'ensemble de son armée:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,728,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils le suivaient, complètement enragés.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,835,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était monté sur un cheval haut au garrot,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,942,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et si gras, vraiment, que par saint Quinaut d'échafaudage,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1049,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On aurait pu lui compter les côtes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1156,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et lui grimper dessus comme sur des lattes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1263,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour recouvrir un toit de tuiles ou de planchettes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1370,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme s'il revenait d'exil.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1477,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ç'était un spectacle épouvantable",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1584,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A contempler, je n'hésite pas à le dire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1691,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec eux il y avait des Hellequines",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1798,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux gracieuses parures,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,1905,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui se divertissaient en chantant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,2012,1197,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/34v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce lai qui débute ainsi :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/34v#xywh=3685,2119,1197,107"
    }
    
  ]
}
