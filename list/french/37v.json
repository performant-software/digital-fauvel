      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3258,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De leurs propres yeux, en grand train,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3361,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Venant de droite et de gauche",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3464,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se rendant sur les lieux de la joute.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3567,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Assurément, je commettrais un grand crime",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3670,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si je n'évoquais pas dans mes vers",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3773,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le fait que la glorieuse Mère",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3876,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Du roi du ciel, par un mystère insondable,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,3979,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Descendit sur nous tous des cieux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,4082,1281,103"
    },
    
    {
