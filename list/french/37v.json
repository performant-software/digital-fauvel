      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la très chaste, très pure et très lumineuse",
