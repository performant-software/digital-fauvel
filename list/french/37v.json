      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,4494,1281,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Virginité, pour qui elle a tant d'affection,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,4597,1281,103"
    },
    
    {
