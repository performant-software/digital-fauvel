      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/37v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Afin d'apporter du réconfort",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/37v#xywh=2583,4391,1281,103"
    },
    
