  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle était revêtue et chaussée de fer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,703,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et équipée comme ille fallait :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,809,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son écu était de couleur noire orné d'une tête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,915,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui a précipité maint homme dans le malheur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1021,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par des insensés. et des plaisirs dépravés.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1127,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour tunique, elle portait un surplis",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1233,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Noir comme la mûre, semé de besants d'or;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1339,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle avait profusion d'autres équipements",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1445,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui étaient du même acabit :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1551,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sa parure était la mort subite.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1657,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son visage fit voir une expression de violente colère",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1763,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle saisit sa lance et la fixa",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1869,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et retourna tout droit aux lices",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,1975,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Accompagnée de plus de vingt Vices.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,2081,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Virginité, qui l'attendait,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,2187,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'accueillit avec ces sarcasmes :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,2293,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Allez, lance ton cheval, sale garce ! »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=1197,2399,1407,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Alors, Sensualité tourna son visage",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,714,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Vers elle et se laissa aller",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,820,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À courir, mais Virginité",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,926,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lui assena un tel coup sur le sein",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1032,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elle tomba en faisant la culbute",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1138,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se retrouva par terre toute ébahie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1244,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Adultère la relève aussitôt",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1350,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tandis qu'un héraut la conspue à grands cris",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1456,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se moque d'elle, même si cela déplaît à d'aucuns",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1562,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parce qu'elle était armée trop lourdement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1668,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et montait un destrier trop grand.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1774,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cela ne l'a desservi, le héraut,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1880,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car le cheval dont elle était tombée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,1986,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lui fut immédiatement donné par Virginité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,2092,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sensualité, elle, interpelle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,2198,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Virginité, car elle veut jouter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,2304,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Derechef: c'est à son corps défendant!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2604,2410,1354,106"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Alors Virginité se rendit compte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4368,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elle n'avait pas brisé sa lance",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4475,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lors de l'assaut précédent qu'elle avait porté.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4582,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En voyant que Sensualité arrivait",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4689,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur elle, elle ne perd pas de temps et s'élance :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4796,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle donna à la bâtarde un tel coup",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,4903,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De sa lance, qui était solide et rigide,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5010,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'elle l'abattit de son cheval, toute chaleur ayant quitté son corps,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5117,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et la jeta à terre complètement étourdie :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5224,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce coup lui brisa la lance.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5331,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur le chemin du retour, elle aperçut Adultère :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5438,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a des raisons de l'affronter",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5545,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et la volonté de le faire, et il en est de même",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5652,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour Adultère, qui a été fâché",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5759,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir Sensualité terrassée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5866,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son désir est que cette journée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=2646,5973,1218,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Apporte quelques soucis à Virginité :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,693,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est alors que se mit à redoubler",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,798,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La joie de Virginité.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,903,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Saint Denis Montjoie ! » s'écrient alors",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1008,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les hérauts, qui voyaient leur avantage.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1113,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Adultère possédait, et c'était suffisant pour lui,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1218,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Un destrier vigoureux et qu'il maîtrisait bien;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1323,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il était pourvu d'armes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1428,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Assez ignobles, comme il va de soi.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1533,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il avait revêtu un justaucorps bien rembourré,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1638,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cadeau de Luxure,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1743,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Maladroitement peint et fabriqué,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1848,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et sa tête était protégée par un heaume",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,1953,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui avait été suspendu en pleine fumée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2058,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme j'en suis convaincu, pendant un an et demi:",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2163,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On avait l'impression que c'était un diable.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2268,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il tenait en main un pieu en guise de lance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2373,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et par malchance et par infortune,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2478,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il avait un bouclier fait d'une porte de bordel.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2583,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On lui ouvrit un grand et vaste couloir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2688,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De sorte qu'il put arriver aux lices.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2793,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il s'imaginait pouvoir se comporter fort honorablement,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,2898,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'il s'est précipité aussitôt dans cette direction;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3003,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il n'est pas fâché de l'affaire !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3108,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Virginité, l'ayant aperçu,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3213,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lui a immédiatement demandé ce qui lui prenait,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3318,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et il lui répondit qu'il ne pouvait échapper",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3423,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À la nécessité de jouter; il se mit alors à piquer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3528,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son grand cheval avec les éperons :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3633,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il tourne son cou dans tous les sens comme un héron,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3738,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se tourne vers Virginité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3843,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais Virginité prend si habilement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,3948,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son élan au moment où il arrive sur elle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4053,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il fallut de peu qu'elle ne l'eût achevé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4158,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par le coup de lance qu'elle lui assena",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4263,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En plein visage, si bien qu'elle l'abattit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4368,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et le jeta à terre, et la chute fut si brutale",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4473,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il s'écria : « Ça manque de paille ici ! »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4578,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel sait tout cela, et en conçoit du dépit :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4683,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il s'en vient au lieu des joutes sans plus tarder",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4788,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Accompagné d'une nombreuse suite de gens",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4893,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout pleins d'ignominie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,4998,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais avant qu'il soit arrivé sur place,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5103,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'affaire s'est déroulée de telle sorte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5208,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que deux de ses sbires, sur lesquels il comptait",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5313,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Beaucoup, se sont complètement couverts de honte.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5418,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chasteté, en effet, avait obtenu la permission",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5523,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De celles qui ont le pouvoir en la matière,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5628,1281,105"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/39v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aller affronter Fornication",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/39v#xywh=3906,5733,1281,105"
    },
    
    {
