    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais il les croise si ingénieusement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,2856,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'on a beaucoup de mal à s'en apercevoir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,2958,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car c'est une bête pleine de malice.",
