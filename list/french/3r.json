      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,3060,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est temps maintenant que je dévoile",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,3162,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
