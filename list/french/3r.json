      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au contraire, il recherche tout ce qui peut lui plaire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,1836,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi, Fauvel, que Dieu m'accorde le salut, ·",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,1938,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne saurait avoir d'autre couleur que fauve,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,2040,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et même, sur son dos, il faut qu'on le sache,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,2142,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne doit pas avoir de raie noire.",
