      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de VOILE, car il a fondé son arrogance",
