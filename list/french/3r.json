      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4896,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et puis en est issue Avarice,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4998,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui n'est pas négligente quand il s'agit de bouchonner Fauvel,",
