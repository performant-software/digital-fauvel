      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4488,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De son nom, par dérivation, on tire Flatterie.",
