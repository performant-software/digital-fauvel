    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour un animal vain, un habit vain.",
