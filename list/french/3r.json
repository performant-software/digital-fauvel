      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,2244,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette sorte de couleur (le fauve) marque la vanité :",
