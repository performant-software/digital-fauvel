      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4590,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui d'aucun bien n'a de fond ni de rive.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4692,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De Fauvel est issue Flatterie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/3r#xywh=1669,4794,1197,102"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/3r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui a la souveraineté sur le monde,",
