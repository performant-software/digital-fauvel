{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/40v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et au nom de Celle pour qui je chante,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,630,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celle-ci ne se délecte de rien d'autre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,738,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que de la perspective de la voir arriver sans tarder.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,846,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle l'attend sur son destrier :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,954,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle n'a pas d'autre désir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1062,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle déclare que la joute entre elles deux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1170,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est appropriée et parfaitement justifiée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1278,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Gloutonnerie, avec des armoiries de couleur noire,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1386,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rejoint sa rangée et saisit sa lance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1494,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et à ce moment, Abstinence s'avance.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1602,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son cheval s'élance et l'autre de même :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1710,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles se donnent réciproquement de si beaux coups,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1818,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'au moment du choc, elles font en sorte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,1926,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que toutes les deux brisent leur lance.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2034,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais Abstinence, au deuxième galop",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2142,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Touche avec sa bannière si brutalement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2250,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dame Gloutonnerie qu'elle en fut abrutie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=1081,2358,1323,108"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ce ne fut pas pour elle une plaisanterie,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,630,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Néanmoins elle se maintint en selle.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,755,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle retourna à son couloir; le cœur farouche,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,880,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour se présenter au troisième assaut.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1005,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Abstinence ne reste pas collée sur place",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1130,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais s'élança en ligne droite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1255,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sur Gloutonnerie, sans se soucier de qui s'en irrite,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1380,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et s'en alla la frapper à·un endroit découvert",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1505,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si fort qu'elle lui ouvrit l'épaule :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1630,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'autre tomba par terre, évanouie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1755,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les gens de Fauvell'ont relevée aussitôt",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,1880,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et emmenée dans leurs loges.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,2005,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils auraient bien aimé être à Limoges,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,2130,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui s'étaient couverts de fer sur ordre de Fauvel",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,2255,1312,125"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/40v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais il était trop tard pour s'en repentir!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/40v#xywh=3738,2380,1312,125"
    }
    
  ]
}
