{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/41v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jamais on n'avait vu des chevaux de cette qualité,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,724,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans aucun pays, par monts et par vaux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,827,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Faits pour gagner dans les joutes,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,930,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des chevaux, surtout, de race,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1033,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme l'étaient ceux des dames ce jour-là,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1136,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qu'elles offrirent sans tarder",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1239,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aux gens de bien qui étaient là en foule",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1342,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et aux jongleurs qui s'y trouvaient.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1445,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi a-t-il déjà volé partout,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1548,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Auprès de tous, le bruit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1651,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que ces dames qui ont jouté",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1754,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sont celles qui doivent occuper la circonférence",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1857,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De l'univers et son centre,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,1960,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans aucune forme d'objection,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2063,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et sans lesquelles aucun homme vivant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2166,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne peut accéder à la voie du salut,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2269,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'à pied et à cheval",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2372,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et par monts et par vaux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2475,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils remplissent routes et chemins,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2578,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui sortent de chez eux pour voir les dames.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2681,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pas de dame ni de seigneur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2784,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ne manifeste sa joie, plus vive",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2887,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que toutes celles qu'on avait vues jusque-là,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,2990,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tant ils se réjouissent de l'arrivée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3093,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des dames et de leur séjour.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3196,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chacun adore le Seigneur pour l'en remercier",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3299,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et fait tout pour montrer sa joie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3402,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De tous côtés les cloches sonnent à la volée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3505,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Résonnent les trompettes, les tambourins, les tambours et les cornemuses ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3608,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les jongleurs font toutes sortes de bêtises ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3711,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Personne ne ménage ses efforts !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3814,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tous s'écrient : «Qu'elles soient les bienvenues !»",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,3917,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et leur visage témoigne de leur grand désir",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4020,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De faire plaisir à ces dames.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4123,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui pouvait leur faire un présent, leur donnait quelque chose",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4226,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et mettait à leur disposition ce qu'il avait :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4329,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des chevaux, des couronnes, des broches,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4432,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des étoffes, des ceintures et de précieux anneaux,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4535,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des hanaps, des coupes d'or et d'argent;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4638,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais elles en prenaient peu, car elles étaient",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4741,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Gens d'honneur et désintéressées,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4844,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ne voulant rien accepter de ce qui est à autrui.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,4947,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elles sont escortées à leurs logis",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5050,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au milieu de considérables témoignages d'honneur,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5153,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et quand arriva le moment de prendre congé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5256,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chacune d'elles sut parfaitement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5359,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Exprimer sa gratitude à qui de droit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5462,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se disait fort obligée",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5565,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Envers eux, d'une affection pure et précieuse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5668,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans aucunement bouchonner Fauvel.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5771,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quant à Fauvel, il avait déjà en catimini",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5874,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Rassemblé tout ce qu'il pouvait de gens",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=1144,5977,1334,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Chez lui, en s'entourant de sa parenté",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,714,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui était nombreuse sur place,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,817,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et Vaine Gloire avec ses amis,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,920,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il les a réunis aux gens de Fauvel :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1023,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Entre eux, ils oublièrent leur honte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1126,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais étaient plus courroucés qu'ils ne laissaient paraître.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1229,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Malgré tout ils firent leur fête",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1332,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne séparèrent plus depuis.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1435,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel coucha si souvent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1538,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Avec Vaine Gloire que celle-ci a eu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1641,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De Fauvel, en nombre incalculable,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1744,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Une foule d'enfants, car elle en engendre en quantité :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1847,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Partout il y a déjà de nouveaux petits Fauvels",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,1950,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui sont bien pires que des louveteaux :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2053,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Son lignage s'est développé à tel point",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2156,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que jamais on n'en avait connu de plus important!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2259,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a bien progressé en ce bas monde,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2362,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a envoyé partout ses amis !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2465,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais par-dessus toute chose,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2568,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je plains le beau jardin rempli de grâce,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2671,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Au sein duquel Dieu, par faveur spéciale,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2774,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A planté la fleur de loyauté",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2877,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et semé au plus haut degré de perfection",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,2980,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La noble graine et la semence",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3083,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la fleur de chrétienté",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3186,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et d'autres en grande abondance :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3289,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les fleurs de paix et de justice,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3392,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La fleur de foi et la fleur de noblesse",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3495,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La fleur d'honneur et la fleur qui s'y est épanouie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3598,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De la sagesse et de la chevalerie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3701,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est né sous une bonne étoile, un jardin",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3804,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui a été paré de fleurs de cette sorte :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,3907,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le jardin de douce France.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4010,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas ! quelle grande infortune",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4113,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir qu'en un verger de cette beauté",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4216,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel est venu prendre ses quartiers.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4319,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si seulement il n'avait jamais eu ni les yeux ni les dents",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4422,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Grâce auxquels il a pu y entrer,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4525,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car la bête remplie de tous les vices",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4628,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Conduit sa femme à travers le jardin,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4731,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il est accompagné de tous ses alliés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4834,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui nous ont pris dans leurs liens pour notre malheur.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,4937,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et parce que cela me met en colère,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,5040,1302,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/41v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Je mets ici ce motet, grognera qui voudra :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/41v#xywh=3801,5143,1302,103"
    }
    
  ]
}
