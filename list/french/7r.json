    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui se présente après lui et ne le vaut guère.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,811,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le bon ne se soucie pas de flatter,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,908,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais l'autre sait bien gratter Fauvel.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1005,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et Fauvel qui distribue les prébendes",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1102,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "N'a aucune considération pour la personne",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1199,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De celui qui remplit correctement son service :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1296,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il ne regarde que celui qui le sert en le frottant.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1393,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est pourquoi ils perdent souvent leur peine",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1490,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ceux qui s'attachent à servir des gens de cette espèce.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1587,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'à nos prélats Dieu donne conseil",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1684,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour qu'ils mettent plus d'enthousiasme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1781,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "À aimer la foi, la raison et la droiture",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1878,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi que ceux qui ont été confiés à leurs soins :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,1975,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont la tête de la sainte Église,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2072,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour autant, voici ce que je leur dis, une fois de plus :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2169,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que par leur notoire défaillance,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2266,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "L'Église, qui jadis fut au plus haut",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2363,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est rabaissée, affligée et humiliée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2460,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comme la barque ployant sous la nasse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2557,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si bien qu'on n'en peut plus tirer aucun profit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2654,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car les pêcheurs sont à blâmer.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2751,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Occupons-nous des autres situations que l'on trouve",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2848,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans l'Église: nous en voyons fort peu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,2945,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui ne soient sortis de l'état où ils devraient se trouver",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3042,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parmi les chanoines, les moines, les clercs et les prêtres.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3139,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les chanoines ont la haine de l'Église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3236,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et s'y rendent peu souvent, sauf quand ils espèrent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3333,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Y recevoir quelque argent ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3430,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ainsi, ils remplissent mal leur devoir.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3527,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le principal de leurs soucis doit être",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3624,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De bien payer à Dieu ce qui lui revient,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3721,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De chanter ses louanges et de l'adorer,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3818,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et au sein de l'Église, d'honorer",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,3915,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Autant le grand que le plus petit.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,4012,1344,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne doivent pas viser en premier lieu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7r#xywh=2887,4109,1344,97"
    }
    
  ]
}
