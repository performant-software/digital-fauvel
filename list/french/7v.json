  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La récompense pécuniaire.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,724,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cela ne devrait pas plaire à Dieu",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,821,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'ils se comportent de cette manière :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,918,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils font tout à l'envers.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1015,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des prébendes, ils en ont largement",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1112,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais ils servent avec parcimonie",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1209,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celui de qui viennent tous les bienfaits.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1306,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas, comme ils exposent à de grands périls",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1403,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En les tenant ainsi, les prébendes de la sainte Église,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1500,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si en retour ils ne rendent pas le type de service",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1597,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'Église a institué !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1694,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'aucuns disent, mais ils ne sont pas sensés,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1791,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que leurs prébendes sont fort libres :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1888,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais c'est faire prendre des chausses noires pour des blanches.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,1985,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils reçoivent les biens de l'Église,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2082,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils les dilapident, les mangent et les boivent",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2179,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et les dégradent de vilaine façon",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2276,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans s'acquitter vis-à-vis de Dieu d'aucun service,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2373,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Dans les lieux où ils doivent séjourner",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2470,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et où ils doivent honorer Dieu.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2567,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Tout bénéfice ecclésiastique",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2664,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Est attribué pour remplir le service divin :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2761,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'il y prenne donc garde celui qui en possède un grand nombre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2858,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car il est embarrassé pour accomplir le service de chacun !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,2955,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont bien parés, les chanoines",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3052,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que saint Benoît le Mal Tourné",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3149,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Compte aujourd'hui dans sa maisonnée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3246,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui mènent une existence pervertie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3343,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne vivent pas à la manière de clercs",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3440,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils vivent au contraire comme des bêtes.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3537,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les uns ont des chevaux aux beaux harnais",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3634,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les autres sont constamment à l'extérieur :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3731,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce sont des fils ou des neveux de Fauvel.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3828,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Leur chevelure est bouclée :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,3925,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont les gendres et non les fils de l'Église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4022,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Car ils jouent trop au petit gentilhomme.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4119,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils portent des coiffes et ont des souliers à lacets,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4216,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais vraiment ils devraient s'écrier « hélas ! »",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4313,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'ils regardaient le crucifix",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4410,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et s'ils se contemplaient souvent en lui,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4507,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En voyant comment le Christ est mis en croix,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4604,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Comment il est étroitement chaussé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4701,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de quel habit il est revêtu !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4798,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne devraient pas être assez entêtés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4895,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ni assez insensés, les chanoines,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,4992,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour gâcher les biens de leur Maître.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,5089,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des prêtres pourvus de cures, que dire ?",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,5186,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Nous allons parler un peu de leur situation :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,5283,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont bien connus en tous lieux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/7v#xywh=2520,5380,1228,97"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/7v",
