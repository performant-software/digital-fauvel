    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils veulent avoir soin des âmes",
