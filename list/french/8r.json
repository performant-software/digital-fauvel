      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2222,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Causent aujourd'hui beaucoup d'ennuis",
