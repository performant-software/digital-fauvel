      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1152,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce ne sont pas de véritables religieux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1259,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les gens de cette espèce, qui sont à ce point désireux",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1366,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De consacrer leur attention au siècle.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1473,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La différence entre le siècle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1580,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et la vie de religieux est grande :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1687,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les deux sont quasi contradictoires.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1794,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Celui qui choisit d'entrer en religion",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,1901,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui renoue avec le siècle qu'il a abandonné,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2008,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Retourne à son vomi comme le chien.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2115,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les moines des ordres mendiants",
