      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2329,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parce qu'ils altèrent leur nature :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2436,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ce sont de pauvres gens pleins de richesses ;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2543,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils renoncent à tout, et veulent tout avoir!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2650,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont hors du monde, et ils ont des préoccupations mondaines.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2757,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On ne connaît plus, que Dieu me vienne en aide !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2864,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De mariage ou de testament,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,2971,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'accord ou de compromis",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,3078,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans qu'y interviennent pour faire les courtiers,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,3185,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les hypocrites, entièrement voués au siècle,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,3292,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ordres mendiants.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8r#xywh=2887,3399,1281,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est un ordre désordonné",
