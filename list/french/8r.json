    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En tout lieu, que ce soit d'hommes ou de femmes ;",
