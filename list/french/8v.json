{
  "@context":"http://iiif.io/api/presentation/2/context.json",
  "@id":"http://fauvel.archivengine.com/list/french/8v.json",
  "@type":"sc:AnnotationList",
  "resources": [
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne détestent rien autant que le cloître :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,777,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Peu nombreux sont ceux qui entrent en religion",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,884,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sans nourrir l'intention pleine et entière",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,991,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "D'avoir des charges et d'améliorer leur position.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1098,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quand après avoir fait un bref séjour au cloître,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1205,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils se hâtent de retourner dans le siècle,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1312,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils sont hors de toute raison et ne sont plus liés par leurs vœux :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1419,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Ils ne craignent ni le blâme ni la honte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1526,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et ne tiennent absolument pas compte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1633,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De ce qu'ils sont consacrés à Jésus-Christ.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1740,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Des moines ils portent l'habit",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1847,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais il y a peu de vertu qui habite",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,1954,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aujourd'hui sous le froc et sous la robe !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2061,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par des gens de cette sorte Dieu est bien tourné en dérision :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2168,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "C'est le seigneur Fauvel qui est leur abbé",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2275,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui très souvent leur sert de prédicateur.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2382,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La bête fauve a fait tant et si bien",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2489,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que les ordres - Dieu me donne le salut éternel !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2596,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Sont presque tous en plein désordre :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2703,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "On voit bien que Fauvel est un mauvais chef;",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2810,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il a mené à la catastrophe un ordre",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,2917,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui était parmi les plus honorés.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3024,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle a tant et si bien fait la bête cruelle",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3131,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que notre mère la sainte Église",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3238,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "S'en plaint amèrement et se lamente :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3345,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "La tricherie des Templiers, bien avérée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3452,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui les a conduits à la mort, à leur perte,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3559,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En a fait une mère triste et affligée.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3666,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Cette affaire la fait soupirer et gémir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3773,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Si fort qu'elle doit en faire frémir tout homme",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3880,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qui est un véritable fils la sainte Église.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,3987,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les Templiers l'ont plongée dans la tristesse,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,4094,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Elle s'en lamente, au fond du désespoir,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,4201,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et se plaint en ces termes :",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2404,4308,1239,107"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Hélas, quelle infortune,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,5680,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Quelle catastrophe et quelle cruelle destinée,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,5795,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De voir que mes enfants m'ont délaissée !",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,5910,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Les Templiers que j'aimais tant",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,6025,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et qui m'avaient témoigné tant d'honneur",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,6140,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "M'ont fait subir outrage et vilenie.",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=2488,6255,1197,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Je les ai nourris et allaités",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,5712,1050,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et dressés dans la limite de mes possibilités",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,5827,1050,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour défendre la Terre sainte",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,5942,1050,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Contre les infidèles Sarrasins,",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,6057,1050,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Mais je vois que maintenant ils sont à ce point dévoyés",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,6172,1050,115"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/8v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Qu'ils ont entrepris de faire la guerre à Jésus-Christ!",
        "language": "french"
      },
      "on": "http://fauvel.archivengine.com/canvas/8v#xywh=3748,6287,1050,115"
    }
    
  ]
}
