      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/9r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Jusqu'à ce que les Templiers reconnussent,",
