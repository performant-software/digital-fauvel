      },
      "on": "http://fauvel.archivengine.com/canvas/9r#xywh=2950,1197,1229,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/9r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Pour obtenir la certitude là-dessus.",
