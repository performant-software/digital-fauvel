    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/9r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Parmi les membres les plus éminents de l'ordre,",
