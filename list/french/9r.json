      },
      "on": "http://fauvel.archivengine.com/canvas/9r#xywh=2950,2227,1229,103"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/9r",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Aussi vaut-il mieux que je me taise.",
