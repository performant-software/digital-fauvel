      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,2292,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Iluecques rois de tous païs",
