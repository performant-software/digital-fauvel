      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et a l'autre main tient le pigne;",
        "language": "original"
