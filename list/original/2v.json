      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1599,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "« Vous dites voir, sire saint Pere! »",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1698,1239,99"
    },
    
    {
