    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Li cardinal dient pour plere :",
