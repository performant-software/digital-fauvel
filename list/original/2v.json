      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,807,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "A cui l'en fet grant reverence,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,906,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Que l'en torche au soir et au main;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1005,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Le pape sili tent la main,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1104,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Par le frain doucement le prent,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1203,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "De torcher nului ne reprent,",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1302,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et puis frate a Fauvel la teste",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1401,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "En disant : « Ci a bele beste ! »",
