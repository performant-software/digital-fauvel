      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,708,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Fauvel regarde en sa presence,",
