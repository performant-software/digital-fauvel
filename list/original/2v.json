      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Lors a Fauvel tost les mains metent",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1797,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Et de lui torcher s'entremetent;",
        "language": "original"
      },
      "on": "http://fauvel.archivengine.com/canvas/2v#xywh=1102,1896,1239,99"
    },
    
    {
      "@type":"oa:Annotation",
      "motivation":"sc:painting",
      "resource": {
        "@id": "http://fauvel.archivengine.com/canvas/2v",
        "@type": "dctypes:Text",
        "format": "text/plain",
        "chars": "Il me semble qu'estrilles tiengnent",
